function doHeadFixup() {
	if (! $(".article_text").length || ! $("#page_head").length || ! $(".article_body").length) {
		setTimeout(doHeadFixup,100);
		return false;
	} 

	if ($(".article_text h5.series").length) {
		$("#page_head").prepend($(".article_text h5.series").eq(0));
	}
	if ($(".article_text h5.photographer").length) {
		$(".article_body .author").append( "<br />" + $(".article_text h5.photographer").eq(0).text() );
		$(".article_body h5.photographer").remove();
	}
		
	$("h1").each(function() {
	  var wordArray = $(this).text().split(" ");
	  if (wordArray.length && wordArray.length > 1) {
	    wordArray[wordArray.length-2] += "&nbsp;" + wordArray[wordArray.length-1];
	    wordArray.pop();
	    $(this).html(wordArray.join(" "));
	  }
	} ) ; 
	 
	 
	 try {
	$("#content_mainA div.article_text p:nth-child(1)").first().after($("#content_mainA .aside"));
	$("#content_mainA div.aside").css({"display":"block"});
