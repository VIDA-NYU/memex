        <script type="text/javascript" charset="utf-8">
        var taxName = 'news.nationalgeographic.com::news::Health';
        var taxBreak = taxName.split("::");
        taxBreak[2] = taxBreak[2].replace(/&amp;/g,"&");
        taxBreak[2] = taxBreak[2].replace(/A-Z/g, "A–Z");
        taxonomy = {site:taxBreak[0],name:taxBreak[2]};
        </script>
        
    

    
        
    

    
