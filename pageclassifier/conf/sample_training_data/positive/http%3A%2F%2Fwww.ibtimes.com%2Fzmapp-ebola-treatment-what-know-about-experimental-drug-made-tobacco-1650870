\/pages.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/print.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/bootstrap-responsive-desktop.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/mediaquery-desktop.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/ie8.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/ie7.css":1,"sites\/www.ibtimes.com\/themes\/us_ibtimes\/css\/ie6.css":1}},"chosen":{"selector":"#edit-field-secondary-categories-und","minimum":"20"},"colorbox":{"opacity":"0.85","current":"{current} of {total}","previous":"\u00ab Prev","next":"Next \u00bb","close":"Close","maxWidth":"98%","maxHeight":"98%","fixed":true,"mobiledetect":true,"mobiledevicewidth":"480px"},"IbtMediaSocial":{"disqus":{"publicKey":"Hw0rWAq2iRpFcVBRcjAHre1gi9PZCx23zsTkS511U7PQpMKZeDQ1NWMjBdYjageW"}},"contentCorrection":{"nodeId":"1650870","correctionFormNodeId":"1696672","correctionFormNodetitle":"ZMapp Ebola Treatment: What To Know About The Experimental Drug Made From Tobacco","correctionFormConfirmationMsg":"\u003Ch3\u003EThank you\u003C\/h3\u003E\u003Cp\u003EYour correction has been received and will be reviewed as soon as possible. IBTimes welcomes your feedback on any item that warrants correction.\u003C\/p\u003E","correctionFormNodeURl":"http:\/\/www.ibtimes.com\/zmapp-ebola-treatment-what-know-about-experimental-drug-made-tobacco-1650870"},"ibtimes":{"page_url":"http:\/\/www.ibtimes.com\/zmapp-ebola-treatment-what-know-about-experimental-drug-made-tobacco-1650870"},"ibtmediaDfp":{"debug":false}});
