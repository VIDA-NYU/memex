    $(document).ready(function() {
        var n = new CDC.LeftNav("nav-primary", "/travel/diseases/ebola", true, true, false);
        n.render();
        $("#nav-informationcenters").parent().children().each(function () {
            if ($(this).get(0).nodeName.toLowerCase() == "ul") {
                $(this).removeClass("ulHide");
                $(this).show();
                $(this).children().each(function () {
                    $(this).removeClass("ulHide");
                    $(this).show();
                });
            }
        });

        $("#nav-destination_clinician").parent().each(function () { $(this).css("background-color", "#E4DABC"); });
        $("#nav-destination_traveler").parent().each(function () { $(this).css("background-color", "#D7E4CC"); });
    });
</script>

