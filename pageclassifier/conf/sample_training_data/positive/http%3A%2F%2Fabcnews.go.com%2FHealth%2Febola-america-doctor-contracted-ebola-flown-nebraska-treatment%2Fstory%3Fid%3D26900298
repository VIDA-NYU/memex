<script type="text/javascript">
addOnload(function(){
renderShareCount(".fbshare .num");
fbInit();
});
var authorCard = new Object();
authorCard.show = function(t_id){
//check first if there are any other hovercards active
if($('.byline .hoverActive').size()>0){
var activeCard = $('.byline .hoverActive');
authorCard.hide(activeCard.attr("id"));
}
var trigger_el = $("#"+t_id);
var target_el = $("#meta_"+t_id);
var target_pos = trigger_el.offset();
var top_pos = target_pos.top - (target_el.height() + trigger_el.height());
target_el.addClass("hoverActive");
target_el.offset({top:top_pos, left:target_pos.left});
target_el.stop().animate({
opacity: .95
}, 300);
}
authorCard.hide = function(t_id){
var trigger_el = $("#"+t_id);
var target_el = $("#meta_"+t_id);
target_el.stop().animate({
opacity: 0
}, 200);
target_el.removeClass("hoverActive");
target_el.css("left", "-1000px");
}
$(document).ready(function() {
window.hoverCardTimer = false;
$("a.authorinfo").mouseenter(function(){
authorCard.show($(this).attr("id"));
});
$("a.authorinfo").mouseleave(function(){
var targetID = $(this).attr("id");
window.hoverCardTimer = setTimeout(function(){
authorCard.hide(targetID);
}, 300)
});
$("a.authorinfo").click(function(e){
e.preventDefault();
authorCard.show($(this).attr("id"));
});
$('.authorMeta').mouseenter(function() {
//console.log('cleartimeout')
clearTimeout(window.hoverCardTimer);
});
$('.authorMeta').mouseleave(function() {
//console.log('hoverActive left')
$(this).stop().animate({
opacity: 0
}, 200);
$(this).removeClass("hoverActive");
$(this).css("left", "-1000px");
});
var topHook = $('.sharetools').offset().top - parseFloat($('.sharetools').css('margin-top'));
var botHook = $('#disqus').offset().top - $('.sharetools').height();
$(window).scroll(function (event) {
var s_pos = $(this).scrollTop();
if(s_pos >= botHook){
$('.sharetools').removeClass('fixed');
} else if (s_pos >= topHook) {
// add fixed class
$('.sharetools').addClass('fixed');
} else {
//remove
$('.sharetools').removeClass('fixed');
}
});
$(".e_video_thumb a").click(function(){
var e_vid_id = $(this).attr("id");
e_vid_id = e_vid_id.replace("e_video_", "");
e_vid_container = "e_video_container_"+e_vid_id;
playStoryVideo(e_vid_container, e_vid_id, true);
});
$(".active_image_closer a").click(function(){
var e_vid_container_id = $(this).closest(".e_video").attr("id");
closeEmbeddedVideo(e_vid_container_id);
});
});
</script>
<script type="text/javascript">
function readCookie(name) {
var nameEQ = name + "=";
var ca = document.cookie.split(';');
for(var i=0;i < ca.length;i++) {
var c = ca[i];
//console.log("c: ",c);
while (c.charAt(0)==' ') {
c = c.substring(1,c.length);
}
if (c.indexOf(nameEQ) == 0) return unescape(c.substring(nameEQ.length,c.length));
}
return null;
}
var disqusCookie = readCookie("DISQUSSESSION");
if(disqusCookie != null) {
var currentTime = (new Date()).getTime();
var base64EncodedStr = readCookie("DISQUSSENC");
var disqusCookieTime = readCookie("DISQUSSESSIONSTART");
if(disqusCookieTime != null && base64EncodedStr != null) {
var diff = (currentTime - disqusCookieTime)/1000;
if(diff > 3600) { // 1 hour 3600
$.ajax ({
url : "/disqus/disqusResignin",
type: 'GET',
data: "getPayload&encStr="+base64EncodedStr,
//Ajax events
beforeSend: function(){},
success: function(response){
disqusCookie = readCookie("DISQUSSESSION");
},
error: function(){},
cache: false,
contentType: false,
processData: false
});
}
}
}
var ie7flag = false
if($.browser.msie ) {
if(parseInt($.browser.version, 10) == 7){
ie7flag = true;
}
}
</script>
