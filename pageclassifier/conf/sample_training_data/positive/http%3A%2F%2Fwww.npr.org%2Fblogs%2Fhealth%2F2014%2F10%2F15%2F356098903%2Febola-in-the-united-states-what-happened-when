        <script type="text/javascript">
            $(document).ready(function () {

                var loadImmediately = false;
                var isMainstage = false;

                    loadImmediately = true;


                if (DFP.shouldRenderForDevice('88')) {
                    DFP.visibilityCheck(function() {
                        DFP.renderForLocation('88', loadImmediately, isMainstage);
                    });
                }

            });
        </script>
