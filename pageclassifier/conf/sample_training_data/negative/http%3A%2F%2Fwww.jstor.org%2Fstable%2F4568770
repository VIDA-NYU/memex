jstor.starPushHelper = function(customParameters) {
    for (var i=0; i < jstor.starBasicIdentities.length; i++) {
        if(i > 0 && typeof jstor.starBasicIdentities[i].consortium != 'undefined' && jstor.starBasicIdentities[i].consortium.indexOf("+") < 0 ){
            jstor.starBasicIdentities[i].consortium = "+" + jstor.starBasicIdentities[i].consortium;
        }
        var combinedOptions = $.extend({}, jstor.starBasicIdentities[i], customParameters);
        combinedOptions = jstor.starSetDatabaseTitle(combinedOptions);
        _$tarQ.push(['track', combinedOptions] );
    };
  
};
jstor.starRecordViewPush = function() {
        if ($("#browseBook").length) {
            jstor.starPushHelper({
                itemtype : "RECORD_VIEW",
                datatype : "BOOK"
            });
            jstor.starPushHelper({
                itemtype : "RECORD_VIEW",
                datatype : "DATABASE"
            });
        } else {
            if (starItemType == "ABSTRACT") {
                scData = $("#SCData").data();
                jstor.starPushHelper({
                    itemtype : "RECORD_VIEW",
                    datatype : "JOURNAL"
                });
                jstor.starPushHelper({
                    itemtype : "RECORD_VIEW",
                    datatype : "DATABASE"
                });
            }
        }
    };
jstor.starSetDatabaseTitle = function (customParameters){if(customParameters.title != "PAMPHLETS" && customParameters.datatype == "DATABASE"){customParameters.title="JSTOR"}return customParameters}
</script>




