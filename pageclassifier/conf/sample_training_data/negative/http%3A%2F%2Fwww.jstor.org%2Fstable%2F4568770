                              if(localStorage.getItem("fromMobile") === "true"){
                                  return true;
                              }
                          }
                          return false;
                      };
                      jstorMobileBookmark.hasHashParameter = function() {
                          var eval = location.href.indexOf("#mobileBookmark") != -1 || location.href.indexOf("#globalNav") != -1;
                          
                          if(eval && window.localStorage){
                              localStorage.setItem("fromMobile", "true");
                          }
                          
                          return eval;
                      };
                      jstorMobileBookmark.setHashParameter = function() {
                          if (!this.hasHashParameter()) {
                              location.href = "#mobileBookmark";
                          }
                      };
                      //1.5 second delay before bubble shows
                      if(!jstorMobileBookmark.mobileReturn()){
