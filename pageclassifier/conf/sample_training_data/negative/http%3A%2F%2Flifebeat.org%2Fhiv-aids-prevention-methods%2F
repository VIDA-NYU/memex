<script>
	$(document).ready(function () {

		//input s focus
		var defaultS = $("input#s").attr('value');
		$("input#s").focus(function () { if ($(this).attr('value')==defaultS) { $(this).attr('value',''); } });
		$("input#s").blur(function () { if ($(this).attr('value')=='') { $(this).attr('value',defaultS); } });

		//submit form
		$("a#asubmit").click(function () {
			$("form#searchform").submit();
			return false;
		});

	});
