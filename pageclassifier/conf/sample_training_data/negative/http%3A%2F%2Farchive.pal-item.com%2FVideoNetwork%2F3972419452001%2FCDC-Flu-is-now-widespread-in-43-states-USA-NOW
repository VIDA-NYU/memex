			_cfg.adurl2 = '';
			_cfg.overlayson= (pageInfo.videooverlays=="on") ? true : false;
			_cfg.vgend= GEL.env.user.gender || "";
			_cfg.vzip= GEL.env.user.zip || "";
			_cfg.vyear= GEL.env.user.yob || "";
			_cfg.Player="1749861674001";
			_cfg.waittime= '';l2 = '';
			_cfg.overlayson= (pageInfo.videooverlays=="on") ? true : false;
			_cfg.vgend= GEL.env.user.gender || "";
			_cfg.vzip= GEL.env.user.zi
			_cfg.homepage= '3972419452001';
			_cfg.videoid='3972419452001';
			_cfg.overlayson= (pageInfo.videooverlays=="on") ? true : false;
			videoPlayer = new GEL.widget.Video.Brightcove(
				GEL.ement("myExperience"), 
				"immersiveplayer", { 
				companion: { 
					companionId: "companionad", 
					animate: false, 
					remnantId: "remnantad", 
