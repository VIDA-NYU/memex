				adserverurlend: '&',
				objectElem: "myExperience",
				player: _cfg.Player,
				overlaysOn: _cfg.overlayson,
				waitfornextvideo: _cfg.waitime || 10000,
				homepage: (!_cfg.homepage) ? true : false,
				videoid: _cfg.videoid,
