			});
			videoPlayer.init();
			if(_D.getElementById("odyArtVideoThumb") && _D.getElementById("odyInitVideoImage")){
				videoPlayer.thumbInit();
			}
			})	
		}
      function getObjectDim(id){
                var e= _D.getElementById(id)
                   ,dims= [1,1]
                ;
                if(!e) 
                        return dims;
                var params= e.getElementsByTagName("param");
                for(var i=0, l= params.length; i < l; i++){
                        if(params[i].name == 'width')
                                dims[0]= params[i].value;
                        else if (params[i].name == 'height')
                                dims[1]= params[i].value ; 
                }
                return dims;
        }
	function startFullpageVideo(){
		try{
			var _f= new GEL.widget.FullPageVideo();
			_f.init();
		}catch(e){
			debugger;
		}
	}
		GEL.thepage.initializer.addInitRoutine({ 
		name: "video",
