<script type="text/javascript" src="http://i.usatoday.net/_common/_scripts/_omniture/om"http://www.usatoday.com/travel/_common/_includes/megamenu.js"></script>
<script type="text/javascript">
var mmObj = mmJson;

function mmFirstItem(objPath, secName){
var objPath = objPath;
var mmFirstItem = "";
mmFirstItem += '<a href="'+objPath[0].Url+'" onclick="s_objectID=\'travelnav|'+secName+'|megamenu|1;\'">';
mmFirstItem += '<span>';
mmFirstItem += '<img class="left mR10" src="'+objPath[0].frontthumb_image_path+'" height="80px" width="80px"/>';
mmFirstItem += '<h5>'+objPath[0].PostTitle+'</h5>';
mmFirstItem += '<p>'+objPath[0].FrontChatter+'</p>';
mmFirstItem += '</span>';
mmFirstItem += '</a>';
return mmFirstItem;
}

function mmUl(objPath, secName){
var objPath = objPath;
var mmUl = "<ul>";
for (var i=1;i<=3;i++) {
mmUl += '<li><a href="'+objPath[i].Url+'" onclick="s_objectID=\'travelnav|'+secName+'|megamenu|'+i+'\';">'+objPath[i].PostTitle+'</a></li>';
}
mmUl += "</ul>";
return mmUl;
}

jQuery(document).ready(function(){
jQuery("#destinationsMMFirstOutput").html(mmFirstItem(mmObj.sections.destinations.items, 'destinations'));
jQuery("#destinationsMMULOutput").html(mmUl(mmObj.sections.destinations.items, 'destinations'));
jQuery("#flightsMMFirstOutput").html(mmFirstItem(mmObj.sections.flights.items, 'flights'));
jQuery("#flightsMMULOutput").html(mmUl(mmObj.sections.flights.items, 'flights'));
jQuery("#cruisesMMFirstOutput").html(mmFirstItem(mmObj.sections.cruises.items, 'cruises'));
jQuery("#cruisesMMULOutput").html(mmUl(mmObj.sections.cruises.items, 'cruises'));
jQuery("#hotelsMMFirstOutput").html(mmFirstItem(mmObj.sections.hotels.items, 'hotels'));
jQuery("#hotelsMMULOutput").html(mmUl(mmObj.sections.hotels.items, 'hotels'));
jQuery("#expertsMMFirstOutput").html(mmFirstItem(mmObj.sections.experts.items, 'experts'));
jQuery("#expertsMMULOutput").html(mmUl(mmObj.sections.experts.items, 'experts'));
});
</script>
<!--/template[pageBottomJS]-->
