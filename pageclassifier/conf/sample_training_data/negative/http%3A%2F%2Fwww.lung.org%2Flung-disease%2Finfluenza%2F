<script type="text/javascript">
//Find promo slots and show first item only
function togglePromos() 
{
                for (i=0;i<=5;i++) 
                {
                                var promoName = 'promo-slot-'+i;
                                if(document.getElementById(promoName)) {//div#promo-slot-N 
                                                pm = document.getElementById(promoName);
                                                for (var ac = pm.childNodes, ai = 0, aj = ac && ac.length || 0, ae; ai < aj; ai++) {
                                                                if (ac[ai].nodeType === 1) { //div#templatelist-N
                                                                                pmc = ac[ai];
                                                                                for (var bc = pmc.childNodes, bi = 0, bj = bc && bc.length || 0, be; bi < bj; bi++) {
                                                                         pmc = ac[ai];                                                                                             if (bc[bi].nodeType === 1) { //div.promo
                                                                                                                pmgc = bc[bi];
                                                                                                                pmgc.style.display="block"; 
                                                                                                                ai=99; //break tempaltelist-N
                                                                                                                break;   //break div.promo                                                                          
                                                                                                }
                                                                                }
                                                                }                                              
                                                }
                                                
                                }
                }
}
togglePromos();
</script>


