<script type="text/javascript">
    var addthisHtml = '<div id="addthis_widget" class="addthis_toolbox addthis_floating_style addthis_32x32_style" style="height:180px;">';
    addthisHtml += '<a class="addthis_button_facebook" style="cursor:pointer"></a>';
    addthisHtml += '<a class="addthis_button_twitter" style="cursor:pointer"></a>';
    addthisHtml += '<a class="addthis_button_linkedin" style="cursor:pointer"></a>';
    addthisHtml += '<a class="addthis_button_google_plusone_share" style="cursor:pointer"></a>';
    addthisHtml += '<a class="addthis_button_compact"  style="cursor:pointer"></a>';
    addthisHtml += '</div>';

    var offset = $('#panel-main').offset();
    $('body').prepend(addthisHtml);

    $('#addthis_widget').css({ position: 'fixed', top: 390, left: offset.left - 45 });

    $(window).resize(function () {
        var offset = $('#panel-main').offset();
        $('#addthis_widget').css({ left: offset.left - 45 });
    });

    $(window).scroll(function () {
        // trigger at scroll 200px past the bottom of #panel-main 
        var addthisMaxScroll = offset.top + $('#panel-main').height() + 200 - $(window).height();
        if ($(window).scrollTop() > addthisMaxScroll) {
            if ($('#addthis_widget').css('display') != "none") {
                $('#addthis_widget').fadeOut(400, function () {
                    // Animation complete.
                });
            }
        }
        else {
            if ($('#addthis_widget').css('display') == "none") {
                $('#addthis_widget').fadeIn(400, function () {
                    // Animation complete.
                });
            }
        }
    });
</script>


        
