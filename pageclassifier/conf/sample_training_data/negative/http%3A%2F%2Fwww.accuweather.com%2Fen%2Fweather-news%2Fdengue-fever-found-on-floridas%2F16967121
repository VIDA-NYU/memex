  <div class="panel">    <div class="panel-head-lt">      <div class="panel-head-rt">        <div class="panel-head"></div>      </div>    </div>    <div class="panel-body-lt">      <div class="panel-body-rt">        <div class="panel-body">          <div class="panel-body-content">            <div class="panel-title">              <h4 class="lt">Daily U.S. Extremes</h4>              <span class="when rt">past 24 hours</span> </div>            -body-rt">        <div class="panel-body">          <div class="panel-body-content">            <div class="panel-title">          <div class="content">              <p>                          <table cellspacing="0" class="stats">                <thead>                  <tr>                    <th>&nbsp;</th>                    <th>Extreme</th>                    <th>Location</th>                  </tr>                </thead>                <tbody>                  <tr class="hi">                    <th scope="row">High</th>                    <td><span id="extremeMaxTemp">N/A</span></td>                    <td><span id="extremeMaxCity"></span><span id="extremeMaxState"></span></td>                  </tr>                  <tr class="lo">                    <th scope="row">Low</th>                    <td><span id="extremeMinTemp">N/A</span></td>                    <td><span id="extremeMinCity"></span><span id="extremeMinState"></span></td>                  </tr>                  <tr class="last pre">                    <th scope="row">Precip</th>                    <td><span id="extremePrecip">N/A</span></td>                    <td><span id="extremePrecipCity"></span><span id="extremePrecipState"></span></td>                  </tr>                </tbody>              </table>              </p>            </div>          </div>        </div>         </div>    </div>    <div class="panel-foot-lt">      <div class="panel-foot-rt">        <div class="panel-foot"></div>      </div>    </div>  </div>
  <script type="text/javascript">
      var isMetric = false;
      function getDailyExtremes() {
          jQuery.ajax({
              url: "/adc2004/pub/includes/headlines/extremes.xml",
              dataType: "xml",
              success: function (xml) {
                  var tempKey = isMetric ? "tempC" : "tempF";
                  var precipKey = isMetric ? "precMM" : "precIN";
                  var precipUnit = isMetric ? "mm" : "\&quot;";
                  $(xml).find("maxTemp").each(function () {
                      jQuery("span#extremeMaxTemp").html($(this).find(tempKey).text() + "\&deg;");
                      jQuery("span#extremeMaxCity").html($(this).find("city").text() + ", ");
                      jQuery("span#extremeMaxState").html($(this).find("state").text());
                  });
                  $(xml).find("minTemp").each(function () {
                      jQuery("span#extremeMinTemp").html($(this).find(tempKey).text() + "\&deg;");
                      jQuery("span#extremeMinCity").html($(this).find("city").text() + ", ");
                      jQuery("span#extremeMinState").html($(this).find("state").text());
                  });
                  $(xml).find("maxPrec").each(function () {
                      jQuery("span#extremePrecip").html($(this).find(precipKey).text() + precipUnit);
                      jQuery("span#extremePrecipCity").html($(this).find("city").text() + ", ");
                      jQuery("span#extremePrecipState").html($(this).find("state").text());
                  });
              }
          });
      }
      getDailyExtremes();
  </script>

