<script type="text/javascript">
    (function () {
        if ($('.awx-story div.rt').length == 0) {
            $('.awx-story blockquote').css({ 'margin': '10px 30px 10px 30px', 'width': 'auto' });
        }
        $('.awx-story img').each(function (i) {
            $(this).removeAttr('align');
        });
        $('#related-links').each(function (i) {
            $(this).removeAttr('style');
            $(this).addClass('clearfix');
            $(this).append('<div class="clearfix"></div>');
            $(this).find('strong').closest('p').remove();
            if ($(this).find('img').length > 0) {
                $(this).prepend('<h6>SOCIAL</h6>');
            } else {
                $(this).prepend('<h6>RELATED</h6>')
            }
        });
    })();				
</script>



