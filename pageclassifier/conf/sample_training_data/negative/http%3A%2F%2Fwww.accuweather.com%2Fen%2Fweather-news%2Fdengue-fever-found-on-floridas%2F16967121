<script type="text/javascript">
function getWeatherWhys() {
    jQuery.ajax({
        url: "http://vortex.accuweather.com/adc2004/pub/includes/headlines/wxwhys_json.asp",
        dataType: "jsonp"
    });
}

function updateWeatherWhy(data) {
    jQuery('p#wxwhys-description').html(data.description);
}

getWeatherWhys();</script>

