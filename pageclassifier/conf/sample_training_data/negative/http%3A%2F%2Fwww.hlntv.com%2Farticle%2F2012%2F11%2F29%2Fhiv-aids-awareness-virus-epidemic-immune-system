          var fbCounter = null ;
          var fbCounter1 = null;
          function updateFbCounter() {
            if (typeof FB !== 'undefined' && fbCounter == null) {
              FB.api({
                method: 'fql.query',
                query:'SELECT total_count FROM link_stat WHERE url = "http://www.hlntv.com/article/2012/11/29/hiv-aids-awareness-virus-epidemic-immune-system"'
              }, function(data) {
                fbCounter = data[0].total_count;
                $('.fb-count').text(fbCounter);        
              });
            }
          }
          
