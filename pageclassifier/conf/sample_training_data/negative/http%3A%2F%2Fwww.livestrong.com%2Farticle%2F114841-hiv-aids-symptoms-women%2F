<script>
				googleAds.addChannel('GoogleAdsense300x250', 'testpool_test2');
			googleAds.addChannel('GoogleAdsense300x250', 'testpool_test2_top');
				googleAds.addChannel('GoogleRelatedAds', 'testpool_test2');
		googleAds.addChannel('GoogleRelatedAds-2', 'testpool_test2');
		googleAds.addChannel('GoogleAdsense336x280', 'testpool_test2');
		googleAds.addChannel('GoogleAdsense336x280', 'testpool_test2_bottom');
			googleAds.addChannel('GoogleAdsense300x250', 'article_top');
		googleAds.addChannel('GoogleAdsense336x280', 'article_bottom');

	var sem_source = false;
var sem_channel = dmjs.cookies.readCookie('');
var sem_client_id = dmjs.cookies.readCookie('');
if(sem_client_id == null)
{
	sem_client_id = "ca-livestrong_html";
}
var google_ad_channel = "";

if(sem_source != false || sem_channel !=null )
{
	if(sem_channel != null)
	{
		google_ad_channel = google_ad_channel.replace(/[^,]+(,?)/, sem_channel + '$1');
	}
	googleAds.overrideAd('GoogleAdsense336x280', { google_ad_client: sem_client_id, google_ad_channel: google_ad_channel });
	googleAds.overrideAd('GoogleRelatedAds', { google_ad_client: sem_client_id, google_ad_channel: google_ad_channel });
	
	 
	var exp_page = dmjs.cookies.readCookie('');
	if(exp_page)
	{
		if(!$){
			dmjs.updateMetaTag('exp_page',exp_page);
		}
		else{
			$("meta[name=exp_page]").attr('content', exp_page);
		}
		dmjs.cookies.createCookie("",exp_page,0,"/");
		dmjs.cookies.createCookie("",sem_client_id,0,"/");
	}
	dmjs.cookies.createCookie("",sem_channel,0,"/");
}

	googleAds.defaults.google_adtest = 'off';
	googleAds.defaults.google_page_url = 'http://www.livestrong.com/article/114841-hiv-aids-symptoms-women/';
	googleAds.render();
</script>
