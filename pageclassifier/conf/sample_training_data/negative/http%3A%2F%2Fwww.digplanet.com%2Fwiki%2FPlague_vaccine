// adjust for small screens
function curwidth() {
	var w = $(window).width();
	if (w < 1178) {
		document.getElementById("t1").style.width = "100px";
		document.getElementById("t2").style.width = "100px";
		document.getElementById("t3").style.width = "100px";
		document.getElementById("t4").style.width = "100px";
		document.getElementById("t5").style.width = "100px";
		//document.getElementById("t6").style.width = "100px";
		document.getElementById("digtabcell").style.minWidth = "600px";
	} else {
		document.getElementById("t1").style.width = "134px";
		document.getElementById("t2").style.width = "134px";
		document.getElementById("t3").style.width = "134px";
		document.getElementById("t4").style.width = "134px";
		document.getElementById("t5").style.width = "134px";
		//document.getElementById("t6").style.width = "134px";
		document.getElementById("digtabcell").style.minWidth = "825px";
	}
}
window.onresize = curwidth;
curwidth();
</script>
