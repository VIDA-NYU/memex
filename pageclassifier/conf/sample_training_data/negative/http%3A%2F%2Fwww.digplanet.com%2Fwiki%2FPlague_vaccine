function toggletabs() {
	if (document.getElementById("digtabcell").style.height != "auto") {
		document.getElementById("digtabcell").style.height = "auto";
		document.getElementById("tabs").style.height = "auto";
		document.getElementById("toglink").innerHTML = "make tab area <b>smaller</b>";
		$("div.tab_container").height("auto");
	} else {
		document.getElementById("digtabcell").style.height = "540px";
		document.getElementById("tabs").style.height = "540px";
		document.getElementById("toglink").innerHTML = "make tab area <b>bigger</b>";
		$("div.tab_container").height("500px");
	}
}

function togglebread() {
	if (document.getElementById("breadblock").style.maxHeight != "400px") {
		document.getElementById("breadblock").style.maxHeight = "400px";
		document.getElementById("togbreadlink").innerHTML = "make breadcrumbs <b>smaller</b>";
	} else {
		document.getElementById("breadblock").style.maxHeight = "100px";
		document.getElementById("togbreadlink").innerHTML = "make breadcrumbs <b>bigger</b>";
	}
}

