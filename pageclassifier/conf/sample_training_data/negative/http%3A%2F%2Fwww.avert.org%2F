<div class="new-feature-block-wrapper"><iframe id="content_videowad" width="1000" height="563" src="//www.youtube.com/embed/dfFzEzubw9Q?rel=0&amp;autoplay=0&amp;controls=0&amp;showinfo=0&amp;enablejsapi=1" frameborder="0" allowfullscreen></iframe><script>
var tag = document.createElement('script');
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
var player;
function onYouTubeIframeAPIReady() {
player = new YT.Player('content_videowad', {
events: {
'onReady': onPlayerReady,
'onStateChange': onPlayerStateChange
}
});
}
function onPlayerReady(event) {
}
var done = false;
function onPlayerStateChange(event) {
$("#imagen").hide();
if (event.data == YT.PlayerState.ENDED && !done) {
done = true;
$("#imagen").show();
}
}
function stopVideo() {
player.stopVideo();
}
</script>

