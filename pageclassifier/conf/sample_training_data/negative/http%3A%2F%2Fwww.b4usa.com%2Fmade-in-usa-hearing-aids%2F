<script language="javascript">
function setupSearchField(searchFld, hasFocus, keypress)
{
	var val = searchFld.value;
	var elem = jQuery(searchFld); 

	elem.removeClass('focus');
	elem.removeClass('hastext');
	
	if(keypress || val!='')
	{
		elem.addClass('hastext');
	}
	else if(hasFocus)
	{
		elem.addClass('focus');
	}
}
jQuery("#s").focus(function() 
{
	setupSearchField(this, true, false);
});
jQuery("#s").blur(function()
{
	setupSearchField(this, false, false);
});
jQuery("#s").keydown(function()
{
	setupSearchField(this, true, true);
});
setupSearchField(document.getElementById("s"), false);
jQuery("#s").focus();
</script>
</body>
</html>
