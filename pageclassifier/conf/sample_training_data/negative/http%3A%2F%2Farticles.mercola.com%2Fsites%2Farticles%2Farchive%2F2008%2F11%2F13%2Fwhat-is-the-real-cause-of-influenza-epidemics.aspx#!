<script type="text/javascript">
    function closeRewardPoints() {
        $("#RewardPoints").dialog('close');
        $("#iFrameRewardPoints").attr("src", "");
        if ($('#SendmsgDiv').hasClass('ui-dialog-content')) {
            $('#SendmsgDiv').dialog('close')
        }
    }
    function BindEvents() {
        $(document).ready(function() {
        if (!detectmobile()) {
            $("#ctl00_bhcr_bbcr_CommentSectionControl_btnTopRated").tooltip({
                track: true,
                delay: 0,
                showURL: false,
                fade: 250,
                bodyHandler: function() {
                    return $($(this).next().html());
                },
                showURL: false
            });

            $("#ctl00_bhcr_bbcr_CommentSectionControl_btnNewest").tooltip({
                track: true,
