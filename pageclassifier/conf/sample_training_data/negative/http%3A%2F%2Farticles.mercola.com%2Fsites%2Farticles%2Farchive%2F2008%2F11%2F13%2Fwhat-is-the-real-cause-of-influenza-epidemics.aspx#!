    addPinterestButton = function(url, media, description) {
        var js, href, html, pinJs;
        pinJs = '//assets.pinterest.com/js/pinit.js';
        //url = escape(url);
        url = encodeURIComponent(url);
        media = encodeURIComponent(media);
        description = encodeURIComponent(description);
        href = 'http://pinterest.com/pin/create/button/?url=' + url + '&media=' + media + '&description=' + description;
        html = '<a href="' + href + '" class="pin-it-button" count-layout="vertical"><img border="0" src="//media.mercola.com/Assets/images/mercola/PinExt.png" title="Pin It" /></a>';
     
        $('td.pinterest').html(html);

        //add pinterest js
        js = document.createElement('script');
        js.setAttribute('data-pin-build', 'parsePinBtns');
        js.src = pinJs;
        js.type = 'text/javascript';
        document.body.appendChild(js);
    }
    
