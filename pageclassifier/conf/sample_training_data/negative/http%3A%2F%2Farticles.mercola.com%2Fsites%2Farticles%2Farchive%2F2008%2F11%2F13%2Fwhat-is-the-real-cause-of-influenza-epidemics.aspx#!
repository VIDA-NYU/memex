                bodyHandler: function() {
                    return $($(this).next().html());
                },
                showURL: false
            });
         }
            IpadFixHeader();

            if (detectmobile()) {
                if (navigator.userAgent.match(/Windows Phone/i) !== null) {
                    $(window).resize(function() {
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnTopRated").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnNewest").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnOldest").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnTopPoster").trigger("mouseleave");
                    });
                }
                else {
                    // Listen for orientation changes
                    window.addEventListener("orientationchange", function() {
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnTopRated").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnNewest").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnOldest").trigger("mouseleave");
                        $("#ctl00_bhcr_bbcr_CommentSectionControl_btnTopPoster").trigger("mouseleave");
                    }, false);
                }
            }



        });
    }
</script>


