        <div id="ctl00_bcr_pnlDrComments">
		
            <a name="drcomment">&nbsp;</a>
            <table border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td class="CommentBg">
                        <br />
                        <div class="MercolaCommentBox">
                            <div>
                                <span id="ctl00_bcr_lblCommentedBy">Dr. Mercola's Comments:</span>
                            </div>
                        </div>
                    </td>
                    <td align="right" class="CommentBg dontprint" width="200">
                        <div style="width: 200px; position: relative; top: 3px;">
                            <a href="http://twitter.com/mercola" id="ctl00_bcr_aTwitter" target="_blank">
                                <img src="//media.mercola.com/themes/mercola/images/Followtwit-Btn.png" border="0"
                                    alt="Follow Dr. Mercola on Twitter" /></a> <a href="http://www.facebook.com/doctor.health" id="ctl00_bcr_aFacebook" target="_blank">
                                        <img src="//media.mercola.com/themes/mercola/images/FollowFB-Btn.png" border="0"
                                            alt="Follow Dr. Mercola on Facebook" /></a>
                        </div>
                    </td>
