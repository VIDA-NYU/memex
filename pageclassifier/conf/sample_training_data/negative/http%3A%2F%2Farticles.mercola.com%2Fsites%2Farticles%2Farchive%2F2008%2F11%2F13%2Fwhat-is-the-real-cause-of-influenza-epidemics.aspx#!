    function toggleSocialButtonsDisplay()
    {
        if(checkBreakPointMobile() === true) 
         {
            if(!$(".printContainer" ).hasClass(".device-hide"))
            {
                $('.printContainer').toggleClass("device-hide",true);            
            }                           
         }
        else
          {
            if($(".printContainer" ).hasClass(".device-hide"))
            { 
                $('.printContainer').toggleClass("device-hide",false); 
            }  
          }  
                        
        if(checkBreakPointMobile() === true) 
          {
            if(!$(".Linkedln_toolbox" ).hasClass(".device-hide"))
            { 
                $('.Linkedln_toolbox').toggleClass("device-hide",true);    
            }                   
          }
        else
          {
            if($( ".Linkedln_toolbox" ).hasClass(".device-hide"))
            {
                 $('.Linkedln_toolbox').toggleClass("device-hide",false); 
            }
          }          
      
    }
    
     $(window).resize(function() {
        if(detectmob())
          {
             toggleSocialButtonsDisplay()
          }
     }).resize();
</script>


 
