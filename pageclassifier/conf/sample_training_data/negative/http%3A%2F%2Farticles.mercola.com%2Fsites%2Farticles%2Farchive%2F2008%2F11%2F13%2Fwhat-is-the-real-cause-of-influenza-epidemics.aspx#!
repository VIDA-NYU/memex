<script type="text/javascript">

    $(document).ready(function() {
        var desc = $('meta[property="og\\:description"]').attr("content");

        if (desc == "" || desc == null) {
            desc = "";
        }
        var urlTrim = window.location.href;
        if (urlTrim.indexOf('?') >= 0) {
            urlTrim = urlTrim.substring(0, urlTrim.indexOf('?'));
        }

        var piniturl = urlTrim;
        var pinitimage = "http://media.mercola.com/assets/images/mercola-preview.gif";
        var pinitdesc = desc + ' ' + piniturl;

        var metapinitimage =  $('meta[property="og\\:image"]').attr("content");;
        if (!metapinitimage) {
        }
        else {
            if (metapinitimage != "" || metapinitimage != null) {
                pinitimage = metapinitimage;
            }
        }
        addPinterestButton(piniturl, pinitimage, pinitdesc);
    });

    function closeEmailToFriend() 
