<script type="text/javascript">
	function pop_load_tab(id, content) {
		if ( typeof id == 'undefined' || id == null && !'id' in id ) return false;
		$('#pop-feed').html(content);
		$('.activetab').addClass('righttab').removeClass('activetab');
		$('#' + id.id).addClass('activetab').removeClass('righttab');
	}
</script>
