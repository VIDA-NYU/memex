<script type="text/javascript">
    function removeHrefsFromNav(){
        $('.dropdown').each(function(){
            if($(this).children('a').text()=='Tools') { $(this).children('a').attr({href:'',onclick:'return false'}) }
            if($(this).children('a').text()=='Marketplace') { $(this).children('a').attr({href:'',onclick:'return false'}) }
            if($(this).children('a').text()=='Special Reports') { $(this).children('a').attr({href:'',onclick:'return false'}) }
        })
    }
</script>
