<div class="twenty-padding">
<h2>About Drugs A-Z</h2>
Drugs A-Z provides drug information from Everyday Health and our partners, as well as ratings from our members, all in one place. Cerner Multum&#8482; provides the data within most of the Basics, Side Effects, Interactions and Dosage tabs. The information within the Reviews and FAQ tabs is proprietary to Everyday Health.<br />
You can browse Drugs A-Z for a specific prescription or over-the-counter drug or look up drugs based on your specific condition. This information is for educational purposes only, and not meant to provide medical advice, treatment, or diagnosis. Remember to always consult your physician or health care provider before starting, stopping, or altering a treatment or health care regimen.
</div>


<script>
  $(document).ready(function(){
    $('.first_review').show();
    $('#side-effects').hide();
    $('#interactions').hide();
    $('#dosage').hide();
    $('#review').hide();
    $('#faq').hide();
    if(window.location.hash.toLowerCase() == "#faq" && $('ul.link_list li#last a').attr('idm') == "review") {
      removeHash();
    }
    console.log($('link_list #last').attr('idm'));
    if(window.location.hash && window.location.hash != '#look_like') {
      if(window.location.hash == '#review-new') {
        window.location.hash = 'review';
        $('#detail_review_drug').click();
      }
      var hash = window.location.hash.split('#')[1];

      current = $('ul.link_list').find('a[idm="' + hash + '"]').parent();
      var current_select = $(".selected")
      current_select.removeClass();
      var current_info = current_select.find('a');
      var current_id = current_info.attr('idm');
        
      $(current).addClass("selected");
      var tab = $(current).find('a');
      var new_id = tab.attr('idm');

      if(new_id == 'basic') {
        removeHash();
      } else {
        window.location.hash = new_id;
      }

      $("#"+current_id).toggle();
      $("#"+new_id).toggle();

      if(window.location.hash == "#review") { set_ugc('1'); }

      Omniture.Init(omniture_zone, omniture_ct, ugc, source_name, auth, app_name, page_id, omnitureevent);
      Omniture.Track();
    }
  });

  function removeHash () { 
      var scrollV, scrollH, loc = window.location;
      if ("pushState" in history)
          history.pushState("", document.title, loc.pathname + loc.search);
      else {
          // Prevent scrolling by storing the page's current scroll offset
          scrollV = document.body.scrollTop;
          scrollH = document.body.scrollLeft;

          loc.hash = "";

          // Restore the scroll offset, should be flicker free
          document.body.scrollTop = scollV;
          document.body.scrollLeft = scrollH;
      }
  }

  $('a.tab_bottom').live("click", function(e) {
    e.stopPropagation();
    e.preventDefault();

    current = $('ul.link_list li.selected').next();
    var old_hash = window.location.hash;
    var current_select = $(".selected")
    current_select.removeClass();
    var current_info = current_select.find('a');
    var current_id = current_info.attr('idm');
      
    $(current).addClass("selected");
    var tab = $(current).find('a');
    var new_id = tab.attr('idm');

    if(new_id == 'basic') {
      removeHash();
    } else {
      window.location.hash = new_id;
    }

    $("#"+current_id).toggle();
    $("#"+new_id).toggle();
    $("body").scrollTop($(".link_list").offset().top);

    if(window.location.hash == "#review") { 
      set_ugc('1');
      ReSetupAds();
      LoadAds();
      var timeTrack = GetTimeSinceDomLoaded(); 
    } else if(old_hash != '#review') {
      set_ugc('0');
    } else {
      set_ugc('0');
      ReSetupAds();
      LoadAds();
      var timeTrack = GetTimeSinceDomLoaded(); 
    }

    Omniture.Init(omniture_zone, omniture_ct, ugc, source_name, auth, app_name, page_id, omnitureevent);
    Omniture.Track();
  });

  // Manage hide/show drug tabs (Basic, side effects, interactions, dosage, reviews)
  $('ul.link_list li a').live("click", function(e) {
    e.stopPropagation();
    e.preventDefault();
    tabs($(this).parent());
  });

  $('a#recent_drug_rating_all').live("click", function(e) {
    e.stopPropagation();
    e.preventDefault();
    tabs($('li a[idm="review"]').parent());
  });
  // $('ul.link_list li').click(function() {
  //  e.preventDefault();
  //  tabs(this);
  // });

  function tabs(current) {
    var old_hash = window.location.hash;
    var current_select = $(".selected")
    current_select.removeClass();
    var current_info = current_select.find('a');
    var current_id = current_info.attr('idm');
      
    $(current).addClass("selected");
    var tab = $(current).find('a');
    var new_id = tab.attr('idm');

    if(new_id == 'basic') {
      removeHash();
    } else {
      window.location.hash = new_id;
    }

    $("#"+current_id).toggle();
    $("#"+new_id).toggle();
    $("body").scrollTop($(".link_list").offset().top);

    if(window.location.hash == "#review") { 
      set_ugc('1');
      ReSetupAds();
      LoadAds();
      var timeTrack = GetTimeSinceDomLoaded(); 
    } else if(old_hash != '#review') {
      set_ugc('0');
    } else {
      set_ugc('0');
      ReSetupAds();
      LoadAds();
      var timeTrack = GetTimeSinceDomLoaded(); 
    }
    
    Omniture.Init(omniture_zone, omniture_ct, ugc, source_name, auth, app_name, page_id, omnitureevent);
    Omniture.Track();
  }
  
  function get_page(url) {
    url = url.split('#')[0];
    if(url.indexOf('?') != -1) {
      var querystr = "&ajax=true"
    } else {
      var querystr = "?ajax=true"
    }
    
    $.get(url+querystr, function(data) {
      $("#review").html(data);
    });
  }
  
  // Ajaxify pagination for drug reviews
  $('.pagination a').live("click", function() {
    get_page(this.href)
    return false;
  });
  
  // Ajaxify link back to drug reviews
  $('#detail_review_drug').live('click', function() {
    get_page(this.href)
    return false;
  });
  
  //Ajaxify permalink ratings link
  $('.permalink-rating').live("click", function() {
    get_page(this.href)
    return false;
  });
  
  //Ajaxify report abuse on the review
  $('.report-abuse').live("click", function() {
    get_page(this.href)
    return false;
  });
  
  //Ajaxify report abuse cancel button
  $('#cancel-abuse').live("click", function() {
    var url = "http://www.everydayhealth.com/drugs/cholera-vaccine#review"
    get_page(url)
    return false;
  });
  
  //Ajaxify review cancel button
  $('#cancel-review').live("click", function() {
    var url = "http://www.everydayhealth.com/drugs/cholera-vaccine#review"
    get_page(url)
    return false;
  });

