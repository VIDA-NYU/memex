        <script type="text/javascript">
            var quancastValue = HISTORY.cookies.get("quancast");
            /*
            if (!quancastValue){
                document.write('<script type="text/javascript" src="http://pixel.quantserve.com/api/segments.json?a=p-84eTroxoNX3JE&callback=qc_results" ><\/script>');
            }

            function qc_results(result) {
                var value="";
                for (var i = 0; i < result.segments.length; i++) {
                    if (result.segments[i].id != 'D' && result.segments[i].id != 'T')
                        value += "Q=" + result.segments[i].id + ";";
                }
                quancastValue = value;

                HISTORY.cookies.setTimeOutDays(1);
                HISTORY.cookies.set("quancast",quancastValue);
            } */
            dartParams = trimParams();

            function trimParams() {
                if (!quancastValue)
                    return dartParams;

                if ((dartParams.length + quancastValue.length) > 470){
                //remove some mv values
                   var splited_array = dartParams.split("mv=");
                   var size = splited_array.length;
                   var afterMv = splited_array[size - 1].substring(splited_array[size -1].indexOf("test="));
                   var temp;

                   //remove some mv values
                   for (var i = 0; i < 3; i++){
                       temp = splited_array[0];
                       for (var j = 1; j < size - i - 2; j++){
                            temp += "mv=" + splited_array[j];
                       }
                       temp += afterMv;
                       if ( (temp + quancastValue) < 470 ){
                           return (temp + quancastValue);
                       }
                   }

                   //if still too long, trim pid
                   splited_array = dartParams.split("pid=");
                   var pid = splited_array[1].substring(0,splited_array[1].indexOf(";") + 1);
                   var afterPid = splited_array[1].substring(splited_array[1].indexOf(";") + 1);
                   if (pid.length > 125)
                        pid = pid.substring(0, 124) + ";";
                   return (splited_array[0] + "pid=" + pid + afterPid + quancastValue);
               }
               else
                    return (dartParams + quancastValue);
            }
        </script>
		
		
			
				
			
				
			
				
			
				
			
		
		

