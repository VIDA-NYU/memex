    function refreshAdsAndAnalytics() {
        var iframe_ids = ['ad-leaderboard-iframe','ad-box-iframe','ad-baseboard-iframe','text-sponsor-iframe'];
        //dart ad calls
        ord = Math.round(Math.random() * 10000000000000000);
        numberOfTiles = 1;
        for (var i = 0; i < iframe_ids.length; i++) {
            var ele = document.getElementById(iframe_ids[i]);
            if (ele) {
                var href_value = ele.src;
            	if (ele.contentWindow.location.href && ele.contentWindow.location.href.length > 0) {
            	    href_value = ele.contentWindow.location.href;
            	}
                if(href_value.indexOf( 'doubleclick' ) == -1 ) {
                    continue;
                }
                
                var newurl = href_value.substr(0, href_value.indexOf('&tile=')) + "&tile=" + tileNumberAndDcopt();
                ele.contentWindow.location.href = newurl;
            }
        }

