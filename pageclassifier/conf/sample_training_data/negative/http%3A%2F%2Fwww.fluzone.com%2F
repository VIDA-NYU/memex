$(document).ready(function() {
  try {

	$('.slideshow').before('<div id="progress" class="left">') .cycle({ 
			fx:    'scrollHorz', 
			speed:  2500,
			pager: '#progress',
     		next: '#rightArrow',
     		prev: '#leftArrow' ,
			pagerAnchorBuilder: function(index, el) {
				return '<a class="ball" onclick="" slide="slide_' + (index+1) + '"></a>'; // create our own pager links so that we don't get the numbers on the pager buttons
			}
 	});	
	// stop the animation if the user clicks on a button in the pager
	$('#progress .ball').click(function() {
		$('.slideshow').cycle('pause');
        var omniture = $.omniture();
        var options = {};
        var slide = $(this).attr('slide');
        omniture.link( options, 'FluzoneDTC:Homepage_'+slide+'_by_pager', 'o');
	}); 
				
	$('#rightArrow, #leftArrow').click(function() {
		$('.slideshow').cycle('pause');
        var slide = $('.activeSlide').attr('slide');
        var omniture = $.omniture();
        var options = {};
        omniture.link( options, 'FluzoneDTC:Homepage_'+slide+'_by_arrow', 'o' );
	}); 

  } catch (e) {
  }

});
</script>

