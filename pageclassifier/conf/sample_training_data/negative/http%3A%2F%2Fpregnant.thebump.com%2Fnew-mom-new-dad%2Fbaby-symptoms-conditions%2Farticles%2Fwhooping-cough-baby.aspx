      ++oldCount;
      $(finalCntr).text(this.addCommas(oldCount));
    },
    addCommas: function(str){
      var parts = (str + "").split("."),
      main = parts[0],
      len = main.length,
      output = "",
      i = len - 1;
      while(i >= 0) {
        output = main.charAt(i) + output;
        if ((len - i) % 3 === 0 && i > 0) {
          output = "," + output;
        }
      --i;
      }
      if (parts.length > 1) {
        output += "." + parts[1];
      }
      return output;
    },

    cross_domain_post: function(url, params) {
      var xdr;
      if (window.XDomainRequest) {
        xdr = new XDomainRequest();
        xdr.open("POST", url);
        xdr.onprogress = function() {};
        xdr.ontimeout = function() {};
        xdr.onerror = function() {};
        xdr.onload = function() {};
        return setTimeout((function() {
          return xdr.send($.param(params));
        }), 0);
      } else {
        return $.post(url, params);
      }
    },

    cross_domain_get: function(url, callback) {
      var xdr;
      if (window.XDomainRequest) {
        xdr = new XDomainRequest();
        xdr.onprogress = function() {};
        xdr.ontimeout = function() {};
        xdr.onerror = function() {};
        xdr.onload = function() {
          var data;
          data = $.parseJSON(xdr.responseText);
          return callback(data);
        };
        xdr.open("GET", url);
        return setTimeout((function() {
          return xdr.send();
        }), 0);
      } else {
        return $.get(url, callback);
      }
    },
    checkCookie: function(cname){
      var value=XO.cookie.getCookie(cname);
      if (value!="") {
        return true;
      }else{
        return false;
      }
    }
  }
})();

