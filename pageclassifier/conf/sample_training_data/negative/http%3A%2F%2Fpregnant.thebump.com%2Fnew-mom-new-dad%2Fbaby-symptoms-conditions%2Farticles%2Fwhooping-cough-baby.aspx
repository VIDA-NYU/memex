XO.pdb = (function() {
  var countLimit = 500;
  var settings = {};
  return {
    init: function(opts, config) {
      for (var x in opts.pdb_objects){
        pdb_object = opts.pdb_objects[x];
        if( $(pdb_object.initStateSelector).length > 0 ){
          this.process( pdb_object );
        }
      }
    },
    process: function(pdb_object){
      var self = this;
      if(this.checkCookie(pdb_object.supporterType)){
        this.changeStatePledged(pdb_object.initStateSelector, pdb_object.finalStateSelector);
      }else{
        $(pdb_object.buttonSelector).click(function(){
          self.pledge(pdb_object);
        });
      }
      this.InitialDisplay(pdb_object);
    },
    pledgeSuccess: function(pdb_object){
      analytics.track(pdb_object.trackingAction, {category: "PDB"});
      this.changeStatePledged(pdb_object.initStateSelector, pdb_object.finalStateSelector);
      this.incrementCount(pdb_object.initCounterSelector, pdb_object.finalCounterSelector);
      XO.cookie.setCookie(pdb_object.supporterType, "exists", 180, "/", XO.siteInfo.domain);
    },
    changeStatePledged: function(initialState, finalState){
      $(initialState).hide();
      $(finalState).show();
      $("#facebookPDBBtn").click(function(){
        analytics.track("facebook share", {category:"PDB"});
      });
      $("#twitterPDBBtn").click(function(){
        analytics.track("twitter share", {category:"PDB"});
      });
