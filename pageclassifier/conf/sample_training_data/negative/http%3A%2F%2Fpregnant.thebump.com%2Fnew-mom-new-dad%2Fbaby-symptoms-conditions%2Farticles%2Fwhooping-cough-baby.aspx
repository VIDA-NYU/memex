      var url = "http://tb-pdb-qa.herokuapp.com/pledges/count?supporter_type="+pdb_object.supporterType;
      this.cross_domain_get(url, function(data){
        var number = self.addCommas(data.count);
        $(pdb_object.bubbleSelector + " div").text(number);
        if(data.count<countLimit){
          $(pdb_object.bubbleSelector).hide();
