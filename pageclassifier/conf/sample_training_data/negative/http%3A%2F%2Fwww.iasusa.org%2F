  <div class="skin-default">
    
    <div id="views_slideshow_cycle_main_front_page_carousel-panel_pane_1" class="views_slideshow_cycle_main views_slideshow_main"><div id="views_slideshow_cycle_teaser_section_front_page_carousel-panel_pane_1" class="views-slideshow-cycle-main-frame views_slideshow_cycle_teaser_section">
