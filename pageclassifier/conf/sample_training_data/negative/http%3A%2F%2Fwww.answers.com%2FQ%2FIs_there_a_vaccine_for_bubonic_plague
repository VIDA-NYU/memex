<article asg-card="question_and_answer"  >
			<section class="frame collapsed ">
		<div class="hgroup collapsed">
									<div class="category">
			<span asg-subtext>In</span>				<a class="category_name" href="http://www.answers.com/T/Conditions_and_Diseases">Conditions and Diseases</a>,				<a class="category_name" href="http://www.answers.com/T/Black_Death_(Plagues)">Black Death (Plagues)</a>			</div>			
						<div class="title_edit_wrapper">
				<h1 class="title"><span class="title_text">Is there a vaccine for bubonic plague?</span> </h1>
				<div class="edit_question" style="display:none;">
					<textarea class="new_title_text">Is there a vaccine fo			
						<div class="title_edit_wrapper">
				<h1 class="title"><span class="title_text">Is there a vaccine for bubonic plague?</span> </h1>
				<div class="edit_qr bubonic plague?</textarea>
					<div class="edit_question_buttons">
						<a class="close_edit_question" href="javascript:void(0);">Cancel</a>
						<button class="edit_question_submit" asg-button="disabled">Save</button>
					</div>
				</div>
				<div class="merge_question" style="display:none;">
