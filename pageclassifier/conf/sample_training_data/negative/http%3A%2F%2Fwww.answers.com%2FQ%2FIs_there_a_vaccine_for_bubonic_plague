<script type="text/javascript">

(function() {
        // Were we requested to only start displaying on certain slide?
    var start_display_on_slide = 0,
        count = 1,

        // DOM Elements
        ad_slot = 'answ_alt_300x250_5',
        ad_slot_wrapper = ad_slot + '_wrapper',

        // Casale configs
        casale_version = '2',
        casale_id = '157478',
        ad_units = '4',
        ad_description = 'Index Exchange: 300x250 (Rectangle)',

        // iframe
        iframe_width = 300,
        iframe_height = 250,

        // Config defined function to get value to assign to
        // varname passed to CasaleArgs.referrer
        get_varname_value = (function() {
            var _map = {"lmi":19658,"page_type":"index\/question_answered","param4":""};

            return function() {
                                return "";
                            }
        })();


    function load_slot() {
        //console.debug("[debug][%s] loading slot...", ad_slot);
        $("#" + ad_slot_wrapper).empty();

        var iframe = document.createElement("iframe");
        var iframe_source = 'javascript:' +
            "'<!DOCTYPE html><html><body leftMargin=\"0\" topMargin=\"0\" marginwidth=\"0\" marginheight=\"0\">" +
            "<sc"+"ript type=\"text\/javascript\"><\/script>" +
            "<!-- " + ad_description + " -->" +
            "<script type=\"text\/javascript\">" +
      <html><body leftMargin=\"0\" topMargin=\"0\" marginwidth=\"0\" marginheight=\"0\">      "  var CasaleArgs = new Object(); " +
            "  CasaleArgs.version = " + casale_version + "; " +
            "  CasaleArgs.adUnits = \"" + ad_units + "\"; " +
                    "  CasaleArgs.casaleID = 157478; " +
                    "<\/script> " +
                    "<script type=\"text\/javascript\" src=\"http:\/\/js.casalemedia.com\/casaleJTag.js\"><\/script>" +
                    "<!-- DO NOT MODIFY -->" +
            "</body></html>'";
        iframe.width = iframe_width + 'px';
        iframe.height = iframe_height + 'px';
        iframe.marginheight="0";
        iframe.marginwidth="0";
        iframe.frameborder="0";
        iframe.scrolling="no";
        iframe.src = iframe_source;

        document.getElementById(ad_slot_wrapper).appendChild(iframe);
        $(document).one("third_party_reload", load_slot);
    }

    function check_for_load() {
      if (count++ == start_display_on_slide) {
            load_slot();
            return false;
        }
        $(document).one('desktop_slide.slidenext', check_for_load);
    }

    $(document).ready(function() {
        if (start_display_on_slide > 1) {
            check_for_load();
        }
        else {
          load_slot();
        }
    });

})();

</script>

