   ion() {
    // Were we requested to only start displaying on certain slide?
    var start_display_on_slide = 0,
        slot_name = 'openx_728x90',
        wrapper_id = slot_name + '_wrapper',
        frame_id = 'd533309fde',
        width = '728px',
        height = '90px',
        auid = '537176496',
        $el = $("#" + wrapper_id),
        count = 1;

    function load_slot() {
//        console.debug("[debug][%s] loading slot...", slot_name);
        $el.empty();
        var iframe = document.createElement("iframe");
        var iframe_source = 'javascript:' +
            '\'<!DOCTYPE html><html><body leftMargin="0" topMargin="0" marginwidth="0" marginheight="0">' +
            "<script type=\"text\/javascript\"><\/script><!--/* OpenX JavaScript tag */--><script type=\"text\/javascript\">if (!window.OX_ads) { OX_ads = []; } OX_ads.push({ \"auid\" : \""+auid+"\" });<\/script> <script type=\"text\/javascript\">document.write(\"<scr\"+\"ipt src=\\\\\"http:\/\/us-ads.openx.net\/w\/1.0\/jstag\\\\\"><\/scr\"+\"ipt>\");<\/script>"+
            "<noscript><iframe id=\"" + frame_id + "\" name=\"" + frame_id + "\" src=\"http:\/\/us-ads.openx.net\/w\/1.0\/afr?auid="+auid+"&cb=INSERT_RANDOM_NUMBER_HERE\" frameborder=\"0\" scrolling=\"no\" width=\"728\" height=\"90\"><a href=\"http:\/\/us-ads.openx.net\/w\/1.0\/rc?cs=d533309fde&cb=INSERT_RANDOM_NUMBER_HERE\" ><img src=\"http:\/\/us-ads.openx.net\/w\/1.0\/ai?auid=537176496&cs=d53330ERT_RANDOM_NUMBER_HERE\" frameborder=\"0\" scrolling=\"no\" width=\"728\" height=\9fde&cb=INSERT_RANDOM_NUMBER_HERE\" border=\"0\" alt=\"\"><\/a><\/iframe><\/noscript>" +
            '</body></html>\'';
        var IRNH = new Date().getTime();
        iframe_source = iframe_source.replace(/INSERT_RANDOM_NUMBER_HERE/g,IRNH);
        iframe.width = width;
        iframe.height = height;
        iframe.marginheight="0";
        iframe.marginwidth="0";
        iframe.frameborder="0";
        iframe.scrolling="no";
        iframe.src = iframe_source;

        document.getElementById(wrapper_id).appendChild(iframe);
        $(document).one("third_party_reload", load_slot);
    }

    function check_for_load() {
      if (count++ == start_display_on_slide) {
          load_slot();
          return false;
        }
        $(document).one('desktop_slide.slidenext', check_for_load);
    }

    // On page load, display the ad
    $(document).ready(function() {
        if (start_display_on_slide > 1) {
            check_for_load();
        }
        else {
          load_slot();
        }
    });
})();

