<script type="text/javascript">
					function round(cpm) {
						var round_value = 0;
						var value = '';
						if(cpm < 1)
						{
							round_value = 20;
						}
						else if(1 <= cpm && cpm < 5.5)
						{
							round_value = 10;
						}
						else if(5.5 <= cpm && cpm < 10)
						{
							round_value = 2;
						}

						if(round_value != 0)
						{
							value = Math.round(cpm * round_value) / round_value;
						}
						else
						{
							value = 10;
						}
						return value.toString();
					}
