// check if an ndn autoplace video has been delivered to #ppixelAutoplace div
var ppixelautodivtimer=setInterval(function(){ppixelautodivcheck()},3000);

function ppixelautodivcheck() {

if ( $('#ppixelAutoplace .ndn_playerContainer').length) {
  // if a video player exists, then give it some margin and a 'related video' label above
  $("#ppixelAutoplaceParent").prepend("<h4 style=\"margin-bottom:2px;font-size:15px;color:#808080;\">Related video on this topic</h4>");
  }

  // stop checking after 30 seconds
  for (var i=0; i<=10; i++)
  {
  if (i=10) {
    clearInterval(ppixelautodivtimer);
    }
  }
}
