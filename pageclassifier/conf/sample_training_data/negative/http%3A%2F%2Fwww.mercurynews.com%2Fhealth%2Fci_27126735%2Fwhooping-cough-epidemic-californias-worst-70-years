<script type="text/javascript">
function getNodePosition(node) {
    var eTop = $(node).offset().top;
    return eTop - $(window).scrollTop();
}

$(document).ready(function() {
    $('#hot-topics').after('<div id="dp-top-stories-location" style="display:block;height:0;"></div><div id="dp-top-stories-bar-wrap"></div>');
    $('#dp-top-stories-bar-wrap').html($('#dp-top-stories-bar-source').html());
    $('#dp-top-stories-bar-source').remove();
    var ci = new RegExp(/[0-9]{7,9}/);
    var ci_match = location.href.match(ci);
    $('#dp-top-stories-bar > .dp-top-stories-item').each( function() {
        if ($(this).children('a').attr('href').match(ci_match)) {
            $(this).remove();
        }
    });
    if ($(window).width() > 1024) {
        var newWidth = ($(window).width() > 1280 ) ? 1260 : ( $(window).width() - 16 );
        $('#dp-top-stories-bar').css('width',newWidth);
    }
    $('#dp-top-stories-bar').slick({
        centerMode: false,
        infinite: true,
        dots: false,
        slidesToShow: 4,
        slidesToScroll: 4,
        speed: 300,
        prevArrow: '<button type="button" class="slick-prev" style="display:none;"><span></span></button>',
        nextArrow: '<button type="button" class="slick-next" style="display:none;"><span></span></button>',
        responsive: [{
            breakpoint: 800,
            settings: {
                arrows: false,
                centerMode: false,
                slidesToShow: 1
            }
        }]
    });
    $('#dp-top-stories-bar button').delay(1000).fadeOut(600);
    $('#dp-top-stories-bar').children().on('mouseenter',function(){
        $('#dp-top-stories-bar button').fadeIn(200);
    });
    $('#dp-top-stories-bar').on('mouseleave',function(){
        $('#dp-top-stories-bar button').fadeOut(450);
    });
    $(window).scroll(function() {
	    if (getNodePosition('#dp-top-stories-location') < 1) {
	        $('#dp-top-stories-bar-wrap').addClass('dp-top-stories-fixed');
                $('#dp-top-stories-location').css('height','72px');
	    } else {
	        if ($('#dp-top-stories-bar-wrap').hasClass('dp-top-stories-fixed')) {
	            $('#dp-top-stories-bar-wrap').removeClass('dp-top-stories-fixed');
                    $('#dp-top-stories-location').css('height','0');
	        }
	    }
	});
    // cachebuster
    if ( $('#dp-top-stories-bar-wrap').height() > 80 ) {
        var cssURL = 'http://local.mercurynews.com/assets/local.css?cache=bust';
        var cssElement=document.createElement('link')
        cssElement.setAttribute('rel','stylesheet')
        cssElement.setAttribute('type','text/css')
        cssElement.setAttribute('href', cssURL);
        document.getElementsByTagName("head")[0].appendChild(cssElement);
    }
});
</script>

