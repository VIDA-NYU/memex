<script type="text/javascript">
// #############
// Begin Most-Popular widget
// #############

// What widgets do we want? What order should they be in? What should they be named?
// The order here is the order the tabs will appear.
// The first of the pair of values is what the tab will be named,
// and the second of the pair is what list will be published.
// These are the available lists: day, hour, news, sports, business, entertainment, style, opinion

var pop_config = {
'quantity': 8,
'location': 'bottom',
'tabs' : {
	'Day': 'day',
	'Hour': 'hour',
	'News': 'news',
	'Sports': 'sports',
	'Biz': 'business',	
	'A&amp;E': 'entertainment',
	//'Photos': 'photo'
	'Email': 'emailed',
	'Life': 'style'
}
};

if ( is_it_home() == true || is_it_article() == true || dfmNav.params.pageTitle === 'San Mateo County Times' || dfmNav.params.pageTitle === 'Berryessa' || dfmNav.params.pageTitle === 'Milpitas Post' || dfmNav.params.pageTitle === 'Pacifica' || dfmNav.params.pageTitle === 'Fremont Bulletin' )
{
    if ( dfmNav.params.pageTitle === 'San Mateo County Times' || dfmNav.params.pageTitle === 'Berryessa' || dfmNav.params.pageTitle === 'Milpitas Post' || dfmNav.params.pageTitle === 'Pacifica' || dfmNav.params.pageTitle === 'Fremont Bulletin' ) pop_config.id = '#region8';

    if ( is_it_home() == true || is_it_article() == true )
    {
        article_pop = setInterval(function(){
            if ( dfm.$('#sidebarmostpop').length ) 
            {
                pop_config.id = '#sidebarmostpop';
                pop_config.location = 'replace';
                pop_config.quantity = 5;
                // If the js has already been loaded we load the widget ourselves
                if ( dfm.$('pop_js').length ) load_pop_widget(pop_config);
                else dfm.$.getScript('http://extras.denverpost.com/cache/popular/mercurynews-widget.js');
                window.clearInterval(article_pop);
            }
        },500);
    }
    var write_mercurynews_widget_photos_hour8 = write_mercurynews_widget_hour8 = write_mercurynews_widget_hourly8 = false;
    dfm.$.getScript('http://extras.denverpost.com/cache/popular/mercurynews-widget.js');
}


// #############
// End Most-Popular widget rewrite
// #############
</script>

