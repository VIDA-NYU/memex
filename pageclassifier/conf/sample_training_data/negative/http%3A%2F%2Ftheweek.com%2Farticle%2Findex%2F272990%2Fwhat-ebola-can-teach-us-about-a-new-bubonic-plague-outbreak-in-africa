<script>
function sticky_relocate() {
  var window_top = jQuery(window).scrollTop()+1;
  var div_top = (jQuery('#sticky-anchor').offset().top);
  var isFirefox = typeof InstallTrigger !== 'undefined'; 
  
   if ((navigator.userAgent.match('iPad')) || (location.search.match('ipad=true'))) {
	  		jQuery('#sticker').css( "display", "block" )
			jQuery('#sticker').css( "position", "absolute" );
			}
   else if(isFirefox) {
		    jQuery('#sticker').css( "display", "block" );
		    jQuery('#sticker').cs || (location.search.match('ipad=true'))) {
	  		jQuery('#sticker').css( "display", "block" )
			jQuery('#sticker').css( "position", "absolute" );
			s( "position", "relative" );
		    }
    else
	        {
			  		if (window_top > div_top)  {
		    		jQuery('#sticker').addClass('stick');
		    		jQuery('#sticker').css( "display", "block" );
					jQuery('#sticker').css( "padding-top", "13px" );
		    		
		  		     } 
		  		    else{ 
		  			jQuery('#sticker').removeClass('stick');
				   }
			}
	}

jQuery(window).load(function() {
  jQuery(window).scroll(sticky_relocate);
  sticky_relocate();
});
</script>



