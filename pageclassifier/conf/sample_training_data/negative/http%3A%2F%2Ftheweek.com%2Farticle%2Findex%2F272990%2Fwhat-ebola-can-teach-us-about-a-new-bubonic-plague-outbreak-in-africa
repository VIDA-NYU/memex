<script>
function sticky_relocate_btns() {
  		var window_top = jQuery(window).scrollTop()+1;
 		var window_bottom = window_top + jQuery(window).height();
  		var div_top = (jQuery('#twitter-social-top').offset().top + 30);
  		var div_bottom = jQuery('#twitter-social-low').offset().top;
  		var social_box_height = jQuery('.sticky_social').height();
  		if((window_top > div_top) && ((window_top + social_box_height) < div_bottom)){
  			jQuery('.sticky_social').fadeIn(200);
  		}
  		else if((window_top + social_box_height) >= div_bottom ){
  			jQuery('.sticky_social').fadeOut(200); }
  		else{
  			jQuery('.sticky_social').fadeOut(200);
  			}
 		}
		
  		jQuery(window).scroll(function() {
  		sticky_relocate_btns();
		});
lse if((window_top + social_box_height) >= div_bottom ){
  			jQuery('.sticky_social').fadeOut(200); }
  		else{
  			jQuery('.sticky_social').fade		jQuery( document ).ready(function() {
        sticky_relocate_btns();
		});




jQuery('.nfInitial').ready(function() {
    jQuery('.nfInitial').each(function(index) {
	    map = {
		'A' : 25, 'B' : 27, 'C' : 27, 'D' : 28, 'E' : 27, 'F' : 27, 'G' : 29, 'H' : 31,
		'I' : 11, 'J' : 12, 'K' : 30, 'L' : 20, 'M' : 40, 'N' : 31, 'O' : 30, 'P' : 29,
		'Q' : 30, 'R' : 29, 'S' : 27, 'T' : 25, 'U' : 30, 'V' : 30, 'W' : 44, 'X' : 29,
		'Y' : 30, 'Z' : 28
		}
		ch = jQuery(this).text();
		width = map[ch];

		oldWidth = jQuery(this).width();
		if (oldWidth != width) {
		    jQuery(this).width(width);
		}
		console.log(index+' '+ch+' '+oldWidth+' => '+width);
	})
});

</script>


 










