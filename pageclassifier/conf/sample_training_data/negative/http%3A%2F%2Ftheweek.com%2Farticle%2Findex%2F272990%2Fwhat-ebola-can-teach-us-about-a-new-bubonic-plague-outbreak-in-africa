<script type="text/javascript">

// set up entire page refresh

var refresh_time = 10000000;
var refresh_timer = setTimeout("", refresh_time);
				
var sectionurl = "world";

// now look to do page hit and ad rotation on this page.  Only need to do if rotationPeriod Set


var rotationCount = 0;

function rotateAds() {
	// drop a click
		
	// rotate ads as needed
	try {
	    googletag.pubads().refresh(rotatableAdList);
	}
	catch (e) {
	}

	if ('0' != 'manual' && 
				'0' != 0) {
		if (rotationCount >= 0) return;
		rotationCount++;

		window.setTimeout(rotateAds, 0000); 
	}
}

</script>
