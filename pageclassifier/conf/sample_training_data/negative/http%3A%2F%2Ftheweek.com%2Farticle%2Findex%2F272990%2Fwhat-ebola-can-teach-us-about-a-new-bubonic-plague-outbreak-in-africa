<script>
var galleryReady = false;
var galleryLaunched = false;
var launchUrl;

function local_tb_show(a, b, c) {
    scroll(0,0);
	tb_show(a, b, c);
}

function launchGallery(url) {
	if (galleryReady || document.readyState == 'complete') {
		local_tb_show(null, url, false);
	} else {
		galleryLaunched = true;
		launchUrl = url;
	}
}


// recordOutboundLink
function rol(link, category, action, newtab) {
	_gaq.push(['_trackEvent', category, action, null, null, true]);
	//alert(link+' '+category+' '+action);
	if (link.href !== undefined) {
	    if (newtab) {
	        setTimeout('window.open("' + link.href + '", "_blank")', 100);
		}
		else {
	        setTimeout('document.location = "' + link.href + '"', 100);
		}
	}
}





// recordOutboundLinkWithAttribute
function rolwa(link, category, action, attribute, newtab) {
	_gaq.push(['_trackEvent', category, action, attribute, null, true]);
	//alert(link+' '+category+' '+action+' '+attribute);
	if (link.href !== undefined) {
	    if (newtab) {
	        setTimeout('window.open("' + link.href + '", "_blank")', 100);
		}
		else {
	        setTimeout('document.location = "' + link.href + '"', 100);
		}
	}
}

// recordOutboundLinkWithAttributeAndValue
function rolwav(link, category, action, attribute, value, newtab) {
	_gaq.push(['_trackEvent', category, action, attribute, parseInt(value), true]);
	//alert(link+' '+category+' '+action+' '+attribute+' '+value);
	if (link.href !== undefined) {
	    if (newtab) {
	        setTimeout('window.open("' + link.href + '", "_blank")', 100);
		}
		else {
	        setTimeout('document.location = "' + link.href + '"', 100);
		}
	}
}


function printArticleBody(reply) {
	//var popup = window.open();  IMPORTANT!! Use TWpopup as window.open returns null here
	popup = TWpopup;

	json = jQuery.parseJSON(reply);
	//alert(json);
	// extract headline and url
	popup.document.write('<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "https://www.w3.org/TR/html4/strict.dtd"><html><head>' +
		'<sc'+'ript type="text/javascript" src="https://7e8c.https.cdn.softlayer.net/807E8C/origin.theweek.com/js/jquery.min.js?209"></sc'+'ript>' +
		'<title>'+json.title+'</title>' +
		'</head><body>'+json.content+'</body></html>');
	popup.document.close();
	return false;
}

function printArticle(articleId) {
	TWpopup = window.open();  // have to use a global as window.open returns null in an ajax callback
    //console.log("printArticle " + articleId);
	new Ajax.Updater('post','/article/formatForPrinting/'+articleId, {asynchronous:true, evalScripts:true, onSuccess:function(transport) {printArticleBody(transport.responseText)}, requestHeaders:['X-Update', 'post']});
	return false;
}


function getQueryString(key, default_)
{
  if (default_==null) default_="";
  key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
  var qs = regex.exec(window.location.href);
  if(qs == null)
    return default_;
  else
    return qs[1];
}

