<div class="back-to-top" id="back-top">
    <a href="#" class="back-to-top"><i class="icon-chevron-sign-up icon-3x"></i></a>
</div>

<script type="text/javascript">
require(["jquery"], function () {
    
    $(window).scroll(function () {
        if ($(this).scrollTop() > 150) {
            $('#back-top').fadeIn();
        } else {
            $('#back-top').fadeOut();
        }
    });
    
    jQuery('.back-to-top').click(function () {
        jQuery('html, body').animate({
            scrollTop: 0
        }, 'slow');
        return false;
    });
});
</script>
<!--end of Scroll back to top-->

<!-- Scroll Depth Start -->
<script type="text/javascript">
	/* Scroll depth tracker plugin from jquery.scrolldepth.min.js. Has to load AFTER google analytics codes */
	require(["scrollDepth"], function() {
		/*Start listening as soon as page loads, but will not send until at 25,50,75,100 percent*/
		jQuery.scrollDepth({
			minHeight: 0, 
			elements: ['body']
		});
	});
</script>
