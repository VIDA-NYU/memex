				$("#reportQuestionAbuse").click(function(e) {
					e.preventDefault();
					if (!flaggedAsAbuse) {
						authenticationWrap(function() {
							jQuery.ajax({
								type: "POST",
								url: "/ajax/submitQuestionAbuse",
