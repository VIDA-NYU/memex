			<script>
			var checkForAnswerNotifications = function(){};
			
			require(["jquery"], function() {
				checkForAnswerNotifications = function (counter){
					jQuery.ajax({
						type: "POST",
						url: '/ajax/answerWaitingCheck',
						dataType: "json",
						success: function(data) {
							if (data.success && (data.newAnswersCount > 0)) {
								$(".notification_bubble").text("" + data.newAnswersCount).show();
								
								if (data.newAnswerNotification && data.newAnswerNotification.qid && data.newAnswerNotification.qhid) {
									$("#new-answer-link").attr("href", "/myquestion/" + data.newAnswerNotification.qid + "?qhid=" + data.newAnswerNotification.qhid);
									
									$("#new-answer-link").click(function() {
										jQuery.ajax({
											type: "GET",
											url: "/ajax/markNotifiink").attr("href", "/myquestion/" + data.newAnswerNotification.qid + "?qhid=" + data.newAnswerNotification.qhid);
									
									$("#new-answer-lication",
											dataType: 'json',
											data: { uuid : data.pinnedAnswerNotification.uuid },
											success: function(data, textStatus, xhr) {}
										});
									});
									
									$("#alert-found-answer").slideDown(750);
									
								} else {
									$("#alert-found-multiple").slideDown(750);
								}
									
								$('.alert-link').click(function() {
									s.events = 'event35';
									s.tl(this,'o','Answer Notification Click');
								});
								
							} else {
								counter++;
								setTimeout("checkForAnswerNotifications(" + counter + ")", counter * 30000);
							}
						}
					});
				}
				
							});
		</script>
	

<script type="text/javascript">
	var adsenseClickAlreadyTracked = false;
	
	function trackAdsenseClick() {
		if (!adsenseClickAlreadyTracked) {
			adsenseClickAlreadyTracked = true;
			var theLink = this;
			
			require(["jquery"], function() {
				var postData = {};
				
				if (window.document && window.document.referrer) {
					postData.refererUri = window.document.referrer;
				}
				
				if (theLink.hasOwnProperty("href")) {
					postData.clickDest = theLink.href;
				}
				
				jQuery.ajax({
					url: "/ajax/trackAdsenseClick",
					type: "POST",
					data: postData,
					dataType: "json"
				});
			});
		}
	}
	
