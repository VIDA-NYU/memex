		var sessionStateDeferredTasks = [];
		var sessionStateLoaded = false;
		var sessionStateData = {};
	
		function sessionStateDefer(task) {
			sessionStateDeferredTasks.push(task);
	
			if (sessionStateLoaded) {
				var eachTask = sessionStateDeferredTasks.shift();
	
				while (typeof eachTask != "undefined") {
					eachTask(sessionStateData);
					eachTask = sessionStateDeferredTasks.shift();
				}
			}
		}
	</script>

