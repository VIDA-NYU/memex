		<script>
			postAuthenticationTask = function() {
				location.reload(true);
			};
			
			require(["jquery"], function() {
				$(".qaAnswer").each(function() {
					var thisAnswer = $(this);
					
					$(this).find("a.rateAnswerUp").click(function(e) {
						e.preventDefault();
						authenticationWrap(function() {
							rateAnswer(thisAnswer.data("answerId"), thisAnswer.data("answererId"), thisAnswer.data("qid"), "POSITIVE", thisAnswer.data("previousVote"));
						});
					});
					
					$(this).find("a.rateAnswerDown").click(function(e) {
						e.preventDefault();
						authenticationWrap(function() {
							rateAnswer(thisAnswer.data("answerId"), thisAnswer.data("answererId"), thisAnswer.data("qid"), "NEGATIVE", thisAnswer.data("previousVote"));
						});
					});
				});
				
				function rateAnswer(answerId, answererId, qid, vote, previousVote) {
					if ((vote == previousVote) || (answererId == currentUserId)) {
						return;
					}
					
					var actualVote = vote;
					var currentRating = parseInt($("#answer-" + answerId + " .answerRatingValue").text());
					
					if (vote == "POSITIVE") {
						currentRating++;
					} else {
						currentRating--;
					}
			rn;
					}
					
					var actualVot		
					if ((previousVote == "NEGATIVE") && (vote == "POSITIVE")) {
						actualVote = "REMOVE_NEGATIVE";
					}
					
					if ((previousVote == "POSITIVE") && (vote == "NEGATIVE")) {
						actualVote = "REMOVE_POSITIVE";
					}
					
					$("#answer-" + answerId).data("previousVote", actualVote);
					$("#answer-" + answerId + " .answerRatingValue").text(currentRating);
					
					require(["json2"], function() {
						jQuery.ajax({
							type: "POST",
							url: "/ajax/rateAnswer",
							dataType: 'json',
							data: { jsonInput : JSON.stringify({"type" : actualVote, "answerId" : answerId, "answererId" : answererId, "qid" : qid}) },
							success: function(data, textStatus, xhr) {}
						});
					});
				}
			});
		</script>
		
				
