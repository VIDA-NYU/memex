	function display_additional_ad ()
		{	document.getElementById('additional-ad').innerHTML +=
				'<table class="house-ads">' +
							'<tr><td>' + getCreativeIFrame('General/MW_GEN_120_LEFT', 'AIDS', 'MD+MB', false) + '</td>' +
							'<td>' + getCreativeIFrame('General/MW_GEN_120_RIGHT', 'AIDS', 'MD+MB', false) + '</td></tr>' +
				'</table>' +
				'<iframe src="/xtra_ad.php?subj=MD%2BMB&search=AIDS&ref=dictionary" frameborder="0" framespacing="0" height="250" width="300" marginheight="0" marginwidth="0"  scrolling="no" style="border: 0; width: 300px; height: 250xp;"></iframe>';
		};

	function report_page_length ()
		{	var the_script = document.createElement('script');
