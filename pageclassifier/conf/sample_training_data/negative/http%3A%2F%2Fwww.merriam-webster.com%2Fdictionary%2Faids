		<script type="text/javascript">

			function stickySubmit (word)
			{
				this.document.location = '/dictionary/' + word;
				return false;
			};
			
			function stickyUpdateCSS ()
			{
				var isDesktop = typeof(window.orientation) == 'undefined'; 

				if ( isDesktop ) {

					// Get the window.
					var win = $('#sticky-auto-container div')[0];
					var box = $('#sticky-header-search-box')[0];
					var pos = $(box).offset();
					
					// Change styles.
					win.style.position	= 'fixed';
					win.style.margin	= '0 0 0 2px';
					win.style.top		= '34px';
					win.style.left		= pos.left + 'px';
				
					// Hide the shadow.
					$(win).find('.autocomplete-w1')[0].style.background = 'none';
				};
			};
			
			jQuery(function()
			{	// This callback handles all suggestion clicks.
				var onAutocompleteSelectHeader = function(value, data)
					{	document.location = "/dictionary/" + value;
					};

				// Autocomplete options.
				var options =
					{	serviceUrl: '/autocomplete',
						width: 222,
						delimiter: /(,|;)\s*/,
						onSelect: onAutocompleteSelectHeader,
						deferRequestBy: 0,
						params: { ref: function() { stickyUpdateCSS(); return 1; } },
						noCache: false // set to true, to disable caching
					};

				// Set up the autocomplete field.
				a1 = $('#sticky-header-search-box').autocomplete(options);	
				
				// Get the autocomplete window.
				var win = $('#' + a1.mainContainerId)[0];
				
				// Reposition it.
				$('#sticky-auto-container')[0].appendChild(win);
				
				// Update the css.
				stickyUpdateCSS();
			});
			
			$(function(){
				var isDesktop = typeof(window.orientation) == 'undefined';

				if ( isDesktop ) { 
					$(window).scroll(function(){
						var scrollTop = $(this).scrollTop();					
						if ( scrollTop > 150 ) {
							var hidden = !$('#sticky-header').hasClass('shown');
							$('#sticky-header').removeClass('hidden').addClass('shown');
							if ( hidden ) {
								$('#sticky-header').css('top', '-80px');
								$('#sticky-header').animate({top: 0}, 350); // hide().fadeIn();
							};
						} else {
							var shown = $('#sticky-header').hasClass('shown');
							$('#sticky-header').removeClass('shown');
							if ( shown ) {
								$('#sticky-header').animate({top: '-80px'}, 350);
							};
						};
					});
					$(window).scroll();
				};
			});
			
			$(function(){
				$(window).resize(function() {
					stickyUpdateCSS();
				});
			});
			
			document.body.insertBefore($('#sticky-header')[0], document.body.childNodes[0]);
			
		</script>		
			
