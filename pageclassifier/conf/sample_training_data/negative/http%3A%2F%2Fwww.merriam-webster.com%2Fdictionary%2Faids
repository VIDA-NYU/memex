<!--// BEGIN SCRIPT FOR GOOGLE ANALYTICS TRACKING //-->
<script type="text/javascript">
	$(window).delegate('.GA_analytics_t', 'click', function(e) {
        var link          = $(this);
        var trackName     = link.attr('data-category');
        var trackPosition = link.attr('data-action');
        var trackDesc     = link.attr('data-label');
        var url           = link.attr('href');
        _gaq.push(['_trackEvent', trackName, trackPosition, trackDesc]); 
	});
	$(window).delegate('.GA_analytics', 'click', function(e) {e.preventDefault();
        var link = $(this);
        var trackName = link.attr('data-category');
        var trackPosition = link.attr('data-action');
        var trackDesc = link.attr('data-label');
        var url = link.attr('href');
   
        _gaq.push(['_set','hitCallback',function() {
		if ( url != '#' ) {
			document.location = url;
		};
        }]);
        _gaq.push(['_trackEvent', trackName, trackPosition, trackDesc]);
	});
</script>
<!--// END SCRIPT FOR GOOGLE ANALYTICS TRACKING //-->
					
		
