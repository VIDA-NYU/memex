function setupAds () 
{
	//alert("Footer:" + document.getElementById('footer').offsetTop);
	//alert("Tower Ad 2 Div:" + document.getElementById('tower-advert2').offsetTop);	
	var ad 				= document.getElementById('footer');
	var footer_offset 	= ad ? ad.offsetTop : 0;

	// Display the second tower advertisement.
	if ( true ||  footer_offset >= 1890 ) { 
		var container = document.getElementById('tower-advert2');
		if ( container ) {
			container.innerHTML = 
				'<iframe src="/towerad2.php?page=' + page_view + '&ord=' + 2147483648*Math.random() + '.' + new Date().getTime() + '&id=MW_DICTIONARY&subj=MD+MB&search=AIDS" frameborder="0" framespacing="0" height="600" width="160" marginheight="0" marginwidth="0"  scrolling="no" style="border: 0; width: 160px; height: 600px; position: absolute; z-index: 999; margin-top: -645px; orig-margin-top: -605px; margin-left: -5px; orig-margin-left: -180px;"></iframe>'; 
		};
	};
};

</script>
