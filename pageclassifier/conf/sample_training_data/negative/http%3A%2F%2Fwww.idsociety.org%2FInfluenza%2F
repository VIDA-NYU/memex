            url: '/Handlers/CommunityFavorites.ashx?action=removeFav&favId=' + favId,
            cache: false,
  .html("<a class=\"icoFavorites\" id=\"addFav\" onclick=\"addFavorites('" + document.title + "','" + document.URL + "')\">Favorites</a>");
            },
            error: function (data) {
                $('#ctl00_ContentPlaceHolder1_PageLinks1_liAddFav').html("<a class=\"icoFavorites\" id=\"addFav\" onclick=\"addFavorites('" + document.title + "','" + document.URL + "')\">Favorites</a>");
            }
        });
        return false;
    }
