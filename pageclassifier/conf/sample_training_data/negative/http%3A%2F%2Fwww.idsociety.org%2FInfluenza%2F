    function addFavorites(pagetitle, pagelink) {
        $.ajax({
            url: '/Handlers/CommunityFavorites.ashx?action=addFav&pagetitle=' + pagetitle + '&pagelink=' + pagelink,
            cache: false,
            success: function (data) {
                $('#ctl00_ContentPlaceHolder1_PageLinks1_liAddFav').html("<a class=\"icoFavorites\" id=\"addFav\" onclick=\"removeFavorites(" + data + ")\">Remove Favorites</a>");
            },
            error: function (data) {
                $('#ctl00_ContentPlaceHolder1_PageLinks1_liAddFav').html("<a class=\"icoFavorites\" id=\"addFav\" onclick=\"removeFavorites(" + data + ")\">Remove Favorites</a>");
            }
        });
        return false;
    }
    function removeFavorites(favId) {
