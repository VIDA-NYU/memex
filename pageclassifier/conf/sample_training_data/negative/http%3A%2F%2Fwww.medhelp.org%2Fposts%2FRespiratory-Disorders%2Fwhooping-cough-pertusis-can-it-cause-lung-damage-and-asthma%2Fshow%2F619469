    function toggleArticle(id) {
        if($p(id).hasClassName('uncollapsed')) {
            $p(id).removeClassName('uncollapsed');
            $p(id).addClassName('collapsed');
            $p('featured_articles').hide();
        } else if($p(id).hasClassName('collapsed')) {
            $p(id).removeClassName('collapsed');
            $p(id).addClassName('uncollapsed');
            $p('featured_articles').show();
        }
    }
    
    function sortSubjects(id) {
        jQuery(".sort_by .option").removeClass("selected");
        jQuery("#"+id).addClass("selected");
    }
    
