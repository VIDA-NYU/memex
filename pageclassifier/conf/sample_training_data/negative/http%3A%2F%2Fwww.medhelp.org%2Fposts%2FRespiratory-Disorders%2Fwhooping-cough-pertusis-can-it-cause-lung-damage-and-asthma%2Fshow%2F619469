            function swtichForum(id) {
                if($p('communities') && $p('communities').hasClassName('selected')) {
                   $p('communities').removeClassName('selected');
                   $p('communities').addClassName('unselected'); 
                }
                if($p('experts') && $p('experts').hasClassName('selected')) {
                   $p('experts').removeClassName('selected');
            sName('selected');
                   $p('communities').addClassName('unselected'); 
                }
                if($p('experts') && $p('experts       $p('experts').addClassName('unselected'); 
                }
                if($p('groups') && $p('groups').hasClassName('selected')) {
                   $p('groups').removeClassName('selected');
                   $p('groups').addClassName('unselected'); 
                }
        
                $p(id).removeClassName('unselected');
                $p(id).addClassName('selected');
        
                $p('communities_content').hide();
                $p('experts_content').hide();
                $p('groups_content').hide();
        
                $p(id+"_content").show();
            }
        </script>


