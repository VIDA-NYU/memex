  window.onload = function(){
	  		 
	  $('#searchTerms').val('');
	  if($('#terms').val()=='Enter keyword or phrase...'){
		  $('#icon').hide(); 
	  }
	  else if($('#terms').val()==''){
		  $('#icon').hide();
	  }
	 
	  else {
		  $('#icon').show();
	  }

