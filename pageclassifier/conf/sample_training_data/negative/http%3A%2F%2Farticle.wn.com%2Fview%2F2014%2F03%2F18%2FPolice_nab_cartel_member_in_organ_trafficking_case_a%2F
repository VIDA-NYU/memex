                function detectTranslateLanguage(country) {
                    jQuery.ajax({
                        url: 'http://upge.wn.com/api/upge/cheetah-photo-search/geoname',
                        data: {country: country, username: 'portsdotcom', style: 'full'},
                        dataType: 'jsonp',
                        type: 'GET',
                        success: function(data, status) {
                            if (data.geonames) {
                                var geonamelangs = data.geonames[0].languages
                                langs = geonamelangs.split(',')
 
                        success: function(da                               if (langs.length == 0) {
                                    jQuery('.btn-translate').slideIn();
                                }
                                jQuery.each(langs, function(i, v) {
                                    if (v.split('-').length > 1) {
                                        v = v.split('-')[0]
                                    }
                                    if (v == 'en') {
                                        return false;
                                    }
                                    if (available_langs[v]) {
                                        jQuery('#translate-group').append('<button id="translate-'+v+'" class="btn-translate btn btn-default" style="">'+available_langs[v].originlang+'</button>')
                                    }
                                });
                                if (jQuery('.btn-translate:visible').length == 0) {
                                    jQuery('.btn-translate').show();
                                }
                            }
                        }
                    });
                }

                function doToken(callback, text, tolang, final_callback) {
                    jQuery.ajax({
                        url: 'http://upge.wn.com/api/upge/cheetah-photo-search/bingtoken',
                        data: {no_cache: 1},
                        dataType: 'jsonp',
                        type: 'GET',
                        beforeSend: function(jhrobj) {
                            jQuery('#translate-action').hide();
                            jQuery('#translate-rollback').hide()
                            jQuery('#translate-loading').show();
                        },
                        success: function(data, status) {
                            if (data.access_token) {
                                callback(data.access_token, text, tolang, final_callback)
                            }
                            else {
                                alert('We have encountered an error with our translation, please retry!');
                            }
                        }
                    });
                }

                function doTranslateLocal(article, tolang, callback) {
                    var d = {
                        articleid: article,
                        to: tolang
                    }
                    jQuery.ajax({
                        url: 'http://upge.wn.com/api/upge/cheetah-article/bingtranslate',
                        data: d,
                        dataType: 'jsonp',
                        type: 'GET',
                    })
            ery.ajax({
                        url: 'htt        .done(function (jqXHR, textStatus, errorThrown) {
                        if (callback != undefined) {
                            callback(jqXHR)
                            // show the rollback button
                            jQuery('#translate-loading').hide();
                            jQuery('#translate-rollback').show()
                        }
                    })
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        alert('Error while translating text ...')
                    });
                }

                function doTranslate(token, text, tolang, callback) {
                    var d = {
                        text: text,
                        to: tolang,
                        //oncomplete: 'ajaxTranslateCallback',
                        appId: "Bearer " + token,
                        contentType: 'text/plain'
                    }
                    jQuery.ajax({
                        url: 'http://api.microsofttranslator.com/V2/Ajax.svc/Translate',
                        data: d,
                        dataType: 'jsonp',
                        type: 'GET',
                        jsonp: 'oncomplete'
                    })
                    .done(function (jqXHR, textStatus, errorThrown) {
                        if (callback != undefined) {
                            callback(jqXHR)
                            // show the rollback button
                            jQuery('#translate-loading').hide();
                            jQuery('#translate-rollback').show()
                        }
                    })
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        alert('Error while translating text ...')
                    });
                }

