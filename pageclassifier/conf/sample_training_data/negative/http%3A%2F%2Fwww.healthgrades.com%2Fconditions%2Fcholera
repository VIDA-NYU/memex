                            $(function(){
                                /*
                                 * This script is inlined to keep encapsulted with DOM elements
                                 * and reduce network calls for addtional scripts when being used 
                                 * as syndicated component
                                 */
                                var $menu;
                                var $target;
                                try{ 
                                    $.getScript("https://user.healthgrades.com/ajax/HgUserObject?desktop=true", function(){
                                        var $portalLogin = $('#portalLogin #userLinks');
                                        if( typeof hgUser !== 'undefined' && hgUser.isLoggedIn){
                                            //if logged in, load bootstrap script that supports logged in menu
                                            $.ajax({
                                                url: hgUser.headerUrl,
                                                success: function(result){  
                                                    $portalLogin.html(result).show();
                                                    
                                                    if (typeof($menu) === 'undefined'){
                                                        $menu = $('#userLinks div .desktopNav');
                                                        $target = $('#userLinks div .btn.btn-inverse.dropdown-toggle');
                                                    }
                                                    
                                                    $('body').click(function(e){
                                                        if (e.target === $target[0] || $('.desktopNav').is(':visible')){
                                                            $menu.toggle();
                                                        }
                                                    });
                                                },
                                                error: function(){ 
                                                    if( typeof console !== 'undefined') {
                                                        console.log("error getting user login structure");
                                                    }
                                                }
                                            });
                                        }
                                        else{
                                            $portalLogin.show(); 
                                        }
                                    });
                                }
                                catch(e){
                                    if( typeof console !== 'undefined' ) {
                                        console.log('error getting user state'); 
                                    }
                                }
                            });
                        </script>
