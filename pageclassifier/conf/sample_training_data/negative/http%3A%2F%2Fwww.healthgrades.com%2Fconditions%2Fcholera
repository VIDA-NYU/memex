<script type="text/javascript">
    (function(){
        /*
        * This script is inlined to keep encapsulted with DOM elements
        * and reduce network calls for addtional scripts when being used 
        * as syndicated component
        */
        /* SHOULD BE MOVED TO SYNDICATED SEARCH JS */
        // Omniture tracking for header search
        var hgForm = $('.hgForm');  
        if( hgForm.length >  0){
            //omniture for header seach
            $.fn.headerSearchTrack = function (what, method, searchType, locationType, prop31) {
                searchType = searchType.toLowerCase();
                what = what.toLowerCase();
                s.prop6 = searchType;
                s.eVar11 = what;
                s.eVar19 = 'search';
                if (method == "" & what != "") {
                    method = "keyword";
                }
                if (method === "") {
                    s.eVar20 = searchType + locationType;
                } else {
                    s.eVar20 = searchType + '|' + method + locationType;
                }
                var prop31Value = "header-search";
                if (typeof prop31 != "undefined") {
                    prop31Value = prop31;
                }
                s.prop31 = s.pageName + "|" + prop31Value;
                s.eVar31 = s.prop31;
                s.linkTrackVars = "prop6,eVar11,eVar19,eVar20,prop31,eVar31,events";
                s.linkTrackEvents = "event1";
                s.events = "event1";
                s.tl(this, 'o', s.pageName + "|search");
            };

            hgForm.on("click", "[data-hgolink='topHospitalSearch']", function () {
                $(this).headerSearchTrack("", "top hospital", $("#multi_search_professionaltype").val().toLowerCase(), $("#multi_sea.pageName + "|search");
            };

            hgForm.on("clirch_location_type").val());
            });
            hgForm.on("click", "[data-hgolink='baseballCardSearch']", function () {
                    $(this).headerSearchTrack("", "name", $("#multi_search_professionaltype").val().toLowerCase(), $("#multi_search_location_type").val(), $(this).data("hgoaction"));
            });
            hgForm.on("click", "[data-hgolink='categorySearch']", function () {
                    $(this).headerSearchTrack("", $(this).data("category"), $("#multi_search_professionaltype").val().toLowerCase(), $("#multi_search_location_type").val(), $(this).data("hgoaction"));
            });

            

            
            //show syndicated searach box
            hgForm.show();
        }
        // End Omniture tracking for header search
        /* SHOULD BE MOVED TO SYNDICATED SEARCH JS */
    })();
    
    $(function(){
        window.setTimeout(function() {    
            var wrapper = $('<div class="hg-syndicated-wrapper">');
            wrapper.append($('#ui-id-2'));
            $('body').append(wrapper);
        }, 500);
    });
