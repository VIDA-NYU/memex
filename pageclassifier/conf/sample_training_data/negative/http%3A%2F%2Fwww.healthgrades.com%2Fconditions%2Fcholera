<script type="text/javascript" lang="JavaScript">
    (function(){
        /*
        * This script is inlined to keep encapsulted with DOM elements
        * and reduce network calls for addtional scripts when being used 
        * as syndicated component
        */
        //update share urls 
        /*
         * These should just be links, not share buttons
         *  
         $('.hg-footer-social a.footer-share').each(function(){
            //get anchor tag
            var $shareElement = $(this);

            //set href to data-href plus current page location NOTE: this is assuming data-href includes initial query string ie: ?url= 
            $shareElement.attr("href", $shareElement.attr("data-href") + $shareElement.attr("data-query") );
        });*/

        //hide seo footer tab content, initalially all shown for SEO
        $('#seoFooterContentContainer, .seoFooterContent').hide();

        // code to hide/show SEO footer links
        var $seoFooterContentContainer = $("#seoFooterContentContainer");
        $("#seoFooterTabs span.seoFooterTabsItem").each(function(idx) {
            $(this).click(function() {
                $seoFooterContentContainer.show();
                $(this).toggleClass("active").siblings().removeClass("active");
                $(".seoFooterContent", $seoFooterContentContainer).hide();
                if ($(this).hasClass("active")) {
                    $(".seoFooterContent:eq(" + idx + ")", $seoFooterContentContainer).fadeToggle("slow")
                } else {
                    $seoFooterContentContainer.slideUp(200, function() {
                        $(this).hide();
                    })
                }
                $("html, body").stop().animate({
                    scrollTop: $(this).offset().top-20
                }, 500);
                return false
            })
        });
    })();
</script>
