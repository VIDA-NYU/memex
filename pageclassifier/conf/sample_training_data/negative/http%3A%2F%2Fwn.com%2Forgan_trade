jQuery(document).ready(function() {
    jQuery('div.collage-thumb').mouseenter(function(e) {
        desc = jQuery(this).find('img').first().attr('title');
        jQuery('div.collage_descriptions > p').html(desc);

        jQuery(this).find('span').first().show();
        jQuery(this).find('span').first().css('display', 'block');
    });
    jQuery('div.collage-thumb').mouseleave(function(e) {
        jQuery('div.collage_descriptions > p').html('');
        jQuery(this).find('span').first().hide();
    });
});
