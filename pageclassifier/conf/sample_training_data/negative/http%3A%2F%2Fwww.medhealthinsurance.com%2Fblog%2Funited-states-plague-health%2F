    var params  = "toolbar=yes,menubar=yes,location=yes,scrollbars=yes,resizable=yes,status=yes,";
    params     += "width=" + screen.width;
    params     += ",height=" + screen.height;
    params     += ",fullscreen=yes"
    var pops    = "/../popup_handler.php" + "?zip=" + zip + "&urltogoto=" + urltogoto;
    window.open(pops, "", params);
  }

  Event.observe(window, 'load', function() {
    Event.observe('button', 'click', function(e){
      if(!isValidZip($F("zip"))){ showMessage(); Event.stop(e); $("zip").focus(); }
    });
  });
</script>			<!-- Author information is disabled per default. Uncomment and fill in your details if you want to use it.
			<li><h2>Author</h2>
			<p>A little something about you, the author. Nothing lengthy, just an overview.</p>
			</li>
			-->

