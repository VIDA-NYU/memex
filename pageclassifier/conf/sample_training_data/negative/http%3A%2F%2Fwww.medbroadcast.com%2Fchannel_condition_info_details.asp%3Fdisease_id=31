		function check_quick_form(submit_type){
			var x = document.forms["quick_search"];
			var errmsg = new Array(), messageout="";
			
			if(x.search.value == ""){
				errmsg[errmsg.length] = "Please enter search value.";
			}else{
				if(x.search.value.length < 3)	errmsg[errmsg.length] = "Search value must have at least 3 characters";
			}
			
			if(errmsg.length > 0){
				for(var i=0;i<errmsg.length;i++){
					messageout+=errmsg[i]+"\n";
				}
				alert(messageout);
				return false;
			}
			else {
				//searchWindow = openWin( 'searching_popup.asp', 'searching_notice', 'width=200,height=100,toolbar=0,location=0,directories=0,status=0,menuBar=0,scrollBars=0,resizable=0' );
				x.action = "search.asp";
				
				if(submit_type == "os"){
					return true;
				}else{
					x.submit();
				}
			}
		}
		
		var addfav = false;
		
		function AddFavCount(){
			var title="Medbroadcast";
			window.external.AddFavorite(location.href,document.title);
			return true;
		}
		
		function DisplayFavLink(){
			var strcurrentlink = location.href
			var pos = strcurrentlink.indexOf("?")
			
			if (pos == -1) {
				window.location=location.href + '?addfav=go'
			}
			else {
				window.location=location.href + '&addfav=go'
			}
		}
	</script>
	
