<script type="text/javascript">
//Code to make sure all health tools links open in new window
//20090601 - MR
$(document).ready(function() {
    $('a').each(function() {
        try {
        if ($(this).attr("href").indexOf("health_tools") > -1 )
            {
                $(this).attr("target","_blank");
            }	
        }
        catch (e) {}														
	});
});
</script>


			
