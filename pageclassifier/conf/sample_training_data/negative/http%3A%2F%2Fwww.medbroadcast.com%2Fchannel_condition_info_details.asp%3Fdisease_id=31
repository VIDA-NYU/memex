<script language="javascript">
		function related_search(submit_type){
			var x = document.forms["quick_related_search"];
			var errmsg = new Array(), messageout="";
			
			if(x.search.value == ""){
				errmsg[errmsg.length] = "Please enter search value.";
			}else{
				if(x.search.value.length < 3)	errmsg[errmsg.length] = "Search value must have at least 3 characters";
			}
			
			if(errmsg.length > 0){
				for(var i=0;i<errmsg.length;i++){
					messageout+=errmsg[i]+"\n";
				}
				alert(messageout);
				return false;
			}
			else {
				x.action = "search.asp?dosearch=related_search_condition";
				
				if(submit_type == "os"){
					return true;
				}else{
					x.submit();
				}
			}
		}
</script>

