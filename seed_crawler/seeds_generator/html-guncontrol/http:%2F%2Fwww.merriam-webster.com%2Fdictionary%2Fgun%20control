<script type="text/javascript">

function recircSubmit (word)
{
	this.document.location = '/dictionary/' + word;
	return false;
};

function recircUpdateCSS ()
{
	var isDesktop = typeof(window.orientation) == 'undefined';

	if ( isDesktop ) {

		// Get the window.
		var win = $('#recirc-auto-container div')[0];
		var box = $('#recirc-header-search-box')[0];
		var pos = $(box).offset();
		
		// Change styles.
		win.style.position	= 'fixed';
		win.style.margin	= '0 0 0 1px';
		win.style.top		= '41px';
		win.style.left		= pos.left + 'px';
	
		// Hide the shadow.
		$(win).find('.autocomplete-w1')[0].style.background = 'none';
	};
};

jQuery(function()
{	// This callback handles all suggestion clicks.
	var onAutocompleteSelectHeader = function(value, data)
		{	document.location = "/dictionary/" + value;
		};

	// Autocomplete options.
	var options =
		{	serviceUrl: '/autocomplete',
			width: 204,
			delimiter: /(,|;)\s*/,
			onSelect: onAutocompleteSelectHeader,
			deferRequestBy: 0,
			params: { ref: function() { recircUpdateCSS(); return 1; } },
			noCache: false // set to true, to disable caching
		};

	// Set up the autocomplete field.
	a1 = $('#recirc-header-search-box').autocomplete(options);	
	
	// Get the autocomplete window.
	var win = $('#' + a1.mainContainerId)[0];

	get_auto_complete_obj = (function (win) {
		var obj = win;
		return function () {
			return obj;
		};
	})(a1);
	
	// Reposition it.
	$('#recirc-auto-container')[0].appendChild(win);
	
	// Update the css.
	recircUpdateCSS();
});

$(function(){
	var isDesktop = typeof(window.orientation) == 'undefined';

	if ( isDesktop ) { 
		$(window).scroll(function(){
			var scrollTop = $(this).scrollTop();					
			if ( scrollTop > 150 ) {
				var hidden = !$('#recirc').hasClass('shown');
				$('#recirc').removeClass('hidden').addClass('shown');
				if ( hidden ) {
					$('#recirc').css('top', '-80px');
					$('#recirc').animate({top: 0}, 350); // hide().fadeIn();
				};
			} else {
				var shown = $('#recirc').hasClass('shown');
				$('#recirc').removeClass('shown');
				if ( shown ) {
					$('#recirc').animate({top: '-80px'}, 350);
					var auto = get_auto_complete_obj();
					if ( auto ) {
						auto.hide();
					};
				};
			};
		});
		$(window).scroll();
	};
});

$(function(){
	$(window).resize(function() {
		recircUpdateCSS();
	});
});

document.body.insertBefore($('#recirc')[0], document.body.childNodes[0]);

</script>

