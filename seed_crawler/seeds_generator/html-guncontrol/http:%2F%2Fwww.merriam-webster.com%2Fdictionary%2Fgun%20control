<script type="text/javascript">

	// This function shows the video overlay.
	function videoOverlay (elem, vid)
	{

		// Get the id.
		var pid = 'mw_test';
		var docurl = "" + document.location.href;
		
		var m2pid = [
			{
				match: /\/(dictionary|thesaurus|medical|concise)\//,
				id: 'mw_results'
			},
			{
				match: /\/video\//,
				id: 'mw_video'
			},
			{
				match: /\/game\//,
				id: 'mw_games'
			}
		];
		
		for ( var index = 0; index < m2pid.length; index++ ) {
			if ( m2pid[index].match.test(docurl) ) {
				pid = m2pid[index].id;
				break;
			};
		};

		// This isn't a desktop; therefore, load the overlay in its own document.
		if ( typeof(window.orientation) !== 'undefined' ) {
			document.location.href = '/video/' + vid + '.html?pid=' + pid;
			
		// The screen height is not big enough for the overlay.
		//} else if ( screen.width <= 800 && screen.height <= 600 ) {
		//	document.location.href = '/video/' + vid + '.html?pid=' + pid;
		
		// This is a desktop machine so we will load the video in the overlay.
		} else {
		
			// Flag this as small desktop.
			if ( screen.width <= 1600 || screen.height <= 900 ) {
				if ( document.body.className.indexOf(' small-desktop') < 0 ) {
					document.body.className += ' small-desktop';
				};
			};
			
			// Flag this as tiny desktop.
			if ( screen.width <= 800 && screen.height <= 600 ) {
				if ( document.body.className.indexOf(' tiny-desktop') < 0 ) {
					document.body.className += ' tiny-desktop';
				};
			};
		
			// Get the overlay.
			var overlay = document.getElementById('video-overlay');
			
			// Create it for the first time.
			if ( !overlay ) {
				// Create it.
				overlay = document.createElement('div');
				overlay.id = 'video-overlay';
				document.body.appendChild(overlay);
				
				// This method closes the overlay.
				overlay.close = function ()
				{
					this.style.display = 'none';
					this.parentNode.removeChild(this);
				};
			};
			
			// Add its content.
			overlay.innerHTML = 
				'<div class="overlay-bkg">&nbsp;</div>' + 
				'<div class="overlay-content">' + 
					'<div>' + 
						'<a id="close-video-btn" href="#" class="overlay-close-btn" onclick="document.getElementById(\'video-overlay\').close(); return false;">&nbsp;</a>' + 
						'<iframe src="/video/' + vid + '.html?pid=' + pid + '" scrolling="no" style="background-color: white;" bgcolor="white"></iframe>' +
					'</div>' + 
				'</div>';
			
			overlay.style.display = 'block';
		};
		
		return false;
	};
	
</script>
