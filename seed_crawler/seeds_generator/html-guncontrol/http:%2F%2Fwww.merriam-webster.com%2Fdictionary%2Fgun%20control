<script type="text/javascript">

	// This method updates the status of the saved words display.
	function updateMySavedWordsDisplay (data)
	{	
		$('#my-saved-words')[0].innerHTML = '';
		
		// Get the My Saved Words component.
		var component = $('#my-saved-words');
		component = component.length > 0 ? component[0] : null;
		
		// Get the Facebook user.
		var fbUser = typeof(getFacebookUser) != 'undefined' ? getFacebookUser() : null;
		
		// Update it.
		if ( component !== null && fbUser != null && fbUser.userID !== null ) {
			console.info('Updating my saved words..');
			var script	= document.createElement('script');
			script.type	= 'text/javascript';
			script.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.in_list&word=/dictionary/gun control&success=onUpdateMySavedWords&failure=onMySavedWordsError&uid=' + escape(fbUser.userID) + '&access_token=' + escape(fbUser.accessToken);			
			document.getElementById('my-saved-words').appendChild(script);					
			
		// The user is not logged in.
		} else {			
			updateMySavedWords(-1, '');
		};
	};

	// This function handles the action of adding a word
	function onAddedToMySavedWords (data)
	{	
		updateMySavedWords(1, data.content.word);
	};

	// This function handles words which couldn't be savd.
	function onMySavedWordsAddError (data)
	{	
		alert("Sorry, your word wasn't saved. Please try reloading this page and saving the word again.");		
	};

	// This function handles all My Saved Words error messages.
	function onMySavedWordsError (data)
	{	
		updateMySavedWords(-1, data.content.word);
	};

	// This function updates the My Saved Words content.
	function onUpdateMySavedWords (data)
	{	
		updateMySavedWords(data.social_id == 'null' ? -1 : (data.content.exists == 1 ? 1 : 0), data.content.word);				
	};
	
	// This function saves a word to the My Saved Words list.
	function deleteFromMySavedWords (container, word)
	{	
		// Get the component.
		var component = $('#my-saved-words');
		component = component.length > 0 ? component[0] : null;
		
		// Get the Facebook user.
		var fbUser = typeof(getFacebookUser) != 'undefined' ? getFacebookUser() : null;
		
		if ( fbUser == null || fbUser.userID == null ) {
			return;
		};
	
		var node	= document.createElement('script');
		node.type	= 'text/javascript';
		node.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.delete&word[]=' + word + '&success=updateMySavedWordsDisplay&failure=updateMySavedWordsDisplay&uid=' + escape(fbUser.userID) + '&access_token=' + escape(fbUser.accessToken)
		container.appendChild(node);		

		// Track the click.
		var url = 'http://www.merriam-webster.com/my-saved-words/dictionary/starclick/unsave/gun control';
		_gaq.push(['_trackPageview', url]);

		return;
	};
	
	// This function saves a word to the My Saved Words list.
	function saveToMySavedWords ()
	{	
		// Get the component.
		var component = $('#my-saved-words');
		component = component.length > 0 ? component[0] : null;
		
		// Get the Facebook user.
		var fbUser = typeof(getFacebookUser) != 'undefined' ? getFacebookUser() : null;
		
		if ( fbUser == null || fbUser.userID == null ) {
			return;
		};
	
		// Create the script tag.
		var node	= document.createElement('script');
		node.type	= 'text/javascript';
		node.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.save&word=/dictionary/gun control&success=onAddedToMySavedWords&failure=onMySavedWordsAddError&uid=' + escape(fbUser.userID) + '&access_token=' + escape(fbUser.accessToken);		
		document.getElementById('my-saved-words').appendChild(node);					

		// Make the cookie "permanent."
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + (400 * 40));
		var c_value=escape('yes') + "; expires="+exdate.toUTCString();
		document.cookie='my_saved_words_seen' + "=" + c_value;

		// Track the click.
		var url = 'http://www.merriam-webster.com/my-saved-words/dictionary1/starclick/save/gun control';
		_gaq.push(['_trackPageview', url]);

		return;
	};
	
	// This method updates the display of the My Saved Words view.
	function updateMySavedWords (which, word)
	{	var no_homograph_tip = !document.getElementById('homograph-tool-tip');
	
		// Get the component.
		var component = $('#my-saved-words');
		component = component.length > 0 ? component[0] : null;
		
		// Nothing to do.
		if ( component == null ) {
			return;
		};		
		
		// Alter the component.
		component.innerHTML = '';
		component.className = which == 1 ? 'word-saved' : 'word-not-saved';
		
		// Create the save(d) text link.
		var node = document.createElement('a');
		node.href = '#';
		node.word = word;
		node.innerHTML = '<span>Save' + ( which == 1 ? 'd' : '' ) + '</span>';
		component.appendChild(node);
		
		// Set some event handlers.
		if ( which == 1 ) {
			node.onclick = function () { 
				if ( typeof('getFacebookUser') != 'undefined' && getFacebookUser().userID != null ) {
					document.location = '/my-saved-words/manage-list.htm'; 
				}; 
				return false;
			};			
			node.onmouseover = function () { 
				var elm = document.getElementById('popularity-info'); 
				if ( elm ) {
					elm.style.display = 'none';
					if ( document.getElementById('homograph-tool-tip') ) {
						document.getElementById('homograph-tool-tip').style.display = 'none'; 
					}; 
				};
			};
		} else if ( which == -1 ) {
			node.onclick = function () { 
				if ( typeof(showFBLogin) != 'undefined' ) {
					showFBLogin(function () {						
						if ( typeof('getFacebookUser') != 'undefined' && getFacebookUser().userID != null ) {
							document.location = '/my-saved-words/manage-list.htm'; 
						}; 
					}); 
				};
				return false; 
			};			
			node.onmouseover = function () {
				document.getElementById('my_saved_words_notice').style.display = 'block'; 
				var elm = document.getElementById('popularity-info');
				if ( elm ) {
					elm.style.display = 'none'; 
				}; 
				if ( document.getElementById('homograph-tool-tip') ) {
					document.getElementById('homograph-tool-tip').style.display = 'none'; 
				}; 
			};
		} else {
			node.onclick = function () {
				if ( typeof(showFBLogin) != 'undefined' ) {
					showFBLogin(function () {
						if ( typeof('getFacebookUser') != 'undefined' && getFacebookUser().userID != null ) {
							document.location = '/my-saved-words/manage-list.htm'; 
						}; 
					}); 
				};
				return false; 
			};
			node.onmouseover = function () {
				document.getElementById('my_saved_words_notice').style.display = 'block';
				var elm = document.getElementById('popularity-info'); 
				if ( elm ) {
					elm.style.display = 'none'; 
				}; 
			};
		};	

		// Create the save(d) star.
		node = document.createElement('a');
		node.href = '#';
		node.className = 'icon';
		node.word = word;
		node.innerHTML = '';		
		component.appendChild(node);
		
		// Set some event handlers.
		if ( which == 1 ) {	
			node.onclick = function () { 
				deleteFromMySavedWords(this.parentNode, this.word); 
				return false; 
			};			
			node.onmouseover = function () { 
				var elm = document.getElementById('popularity-info'); 
				if ( elm ) { 
					elm.style.display = 'none'; 
					if ( document.getElementById('homograph-tool-tip') ) {
						document.getElementById('homograph-tool-tip').style.display = 'none'; 
					}; 
				};
			};
		} else { 
			if ( which == -1 ) {
				node.onclick = function () { 
					if ( typeof(showFBLogin) != 'undefined' ) {
						showFBLogin(saveToMySavedWords); 
					};
					return false; 
				};				
			} else {
				node.onclick = function () { 
					saveToMySavedWords(); 
					return false; 
				};
			};
			
			node.onmouseover = function () { 
				document.getElementById('my_saved_words_notice').style.display = 'block'; 
				var elm = document.getElementById('popularity-info'); 
				if ( elm ) {
					elm.style.display = 'none';
				}; 				
				var exdate=new Date();
				exdate.setDate(exdate.getDate() + (400 * 40));
				var c_value=escape('yes') + "; expires="+exdate.toUTCString();
				document.cookie='my_saved_words_seen' + "=" + c_value;	
			};
		};
		
		// Display the notice.
		if ( !/(^|;\s*)my_saved_words_seen=/.test(document.cookie) && which != 1 ) {
			node			= document.createElement('div');
			node.id			= 'my_saved_words_notice';
			node.className	= 'notice';			
			node.innerHTML = '<a id="my_saved_words_close_button" href="#" onclick="document.getElementById(\'my_saved_words_notice\').clear(); this.parentNode.style.display = \'none\'; return false;" class="close-window" style="position: absolute; margin-left: 263px; margin-top: -6px;"><img src="http://stats.merriam-webster.com/images/popularity/close-window.jpg" /></a>';
			node.innerHTML += '<strong>Save this word to your Favorites.</strong><br />If you\'re logged into Facebook, you\'re ready to go.';
			node.clear = function () { /** if ( document.getElementById('my_saved_words_close_button') ) { document.getElementById('my_saved_words_close_button').style.display = 'none'; }; this.onmouseout = function () { this.style.display = 'none'; }; **/ };
			component.appendChild(node);				
		};
	};	

	// This method updates the My Saved Words feature.
	facebookStatusChangeCallback = (function(){
		var old_handler = typeof(facebookStatusChangeCallback) !== 'undefined' ? facebookStatusChangeCallback : null;
		return function(response) { 
			// Update the My Saved Words component.
			console.info("*** My Saved Words: " + response.status);			
			
			// Get the component.
			var component = $('#my-saved-words');
			component = component.length > 0 ? component[0] : null;
			
			// Create the My Saved Words control if needed.
			if ( component == null ) {
				// Initialize the component.
				component			= document.createElement('div');
				component.id		= 'my-saved-words';
				component.className	= 'not-saved';
				
				// Add the component to its parent.
				var parent = $('.main_content_area');
				parent = parent.length > 0 ? parent[0] : null;
				if ( parent != null ) {
					parent.insertBefore(component, parent.childNodes[0]);
				};								
			};
			
			// Update the component.
			updateMySavedWordsDisplay();
			
			// Call the older handler.
			if ( old_handler !== null ) {
				old_handler(response);
			};
		};
	})();		
	
	// Initialize things.
	facebookStatusChangeCallback();
	
	console.info('*** Loaded My Saved Words module.');
		
</script>        
        <div class="lower_content_container">
