                <div id="floating-advert" class="floating-advert relative-position">
                <script type="text/javascript">
                    // <!--
                
                        output_creative('div-gpt-ad-894456462400224661-5');
                
                    // -->
                </script>		
                </div>
                                                        <script type="text/javascript">
                                                $(function(){
                                                        $(window).scroll(function(){
								// Get the footer.
								var footer = $('#footer');
								footer = footer.length > 0 ? footer[0] : null;

								// Get the floating advertisment.
								var floating_ad = $('#floating-advert');
								floating_ad = floating_ad.length > 0 ? floating_ad[0] : null;

								// Store the floating ad's original offset.
								if ( typeof(floating_ad.start_offset) == 'undefined' ) {
									floating_ad.start_offset = $(floating_ad).offset().top;
								};

								// The footer exists.
								if ( floating_ad != null && footer != null ) {
									// Get the current scroll position.
									var scroll_top = $(this).scrollTop();

									// Get the current footer position.
									var offset_top = $(footer).offset().top - 488;

									var offset_y_incr = 0;

									if ( $('.adslot_top_728x90').length > 0 ) {
										offset_top -= 100;
										offset_y_incr = 250;
									};

									// Get the current advertisment y position.
									var float_ad_top = $(floating_ad).offset().top;					

									// The user has scrolled beyond the ad, so move it.
									if ( scroll_top > (offset_top + offset_y_incr) ) {
										//console.info('Absolute: ' + scroll_top + ' > ' + float_ad_top);
										floating_ad.style.position = 'absolute';
                                                                                floating_ad.style.top = offset_top + 'px';	
									} else if ( scroll_top >= (floating_ad.start_offset + 145) ) { 
										//console.info('Fixed: ' + scroll_top + ' > ' + floating_ad.start_offset);
										floating_ad.style.position = 'fixed';
										floating_ad.style.top = '75px';
									} else {
										//console.info('Relative: ' + scroll_top + ' > ' + float_ad_top);
										floating_ad.style.position = 'relative';
                                                                                floating_ad.style.top = '0';
									};
								};
								
                                                        });
							$(window).scroll();
                                                });
                                        </script>
            </div>
        </div>
        
        <div id="footer" class="spacer_dots" style="margin:0 40px;padding:0;"></div>
		
