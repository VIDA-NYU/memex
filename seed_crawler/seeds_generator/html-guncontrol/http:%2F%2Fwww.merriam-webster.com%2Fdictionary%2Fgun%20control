<script type="text/javascript">

	// This function returns the current Facebook user id and access token.
	function getFacebookUser ()
	{
		return {
			userID: null,
			accessToken: null
		};
	};

	// This function determines if the user is current logged into Facebook.
	function loggedIntoFacebook ()
	{
		return getFacebookUser().userID != null;
	};
	
	// This function shows the Facebook login window.
	function showFBLogin (callback) {
		try { 
			FB.login(function (response) {;
					facebookStatusChangeCallback(response); 
					if ( callback != null ) {
						callback(response);
					};
				}, 
				{perms:''}
			);
		} catch (exp) { 
		}; 

		return false;
	};

	// This method updates the My Saved Words feature.
	facebookStatusChangeCallback = (function(){
		var old_handler = typeof(facebookStatusChangeCallback) !== 'undefined' ? facebookStatusChangeCallback : null;
		return function(response) { 
			// Create the new Facebook user function.
			getFacebookUser = (function (response) {
				var userID = null;
				var accessToken = null;
				
				if ( typeof(response.authResponse) != 'undefined' && typeof(response.authResponse.userID) !== 'undefined' ) {
					userID = response.authResponse.userID;
					accessToken = response.authResponse.accessToken;
				};
				
				return function () {
					return {
						userID: userID,
						accessToken: accessToken
					};
				};
			})(response);
			
			// Call the older handler.
			if ( old_handler !== null ) {
				old_handler(response);
			};
		};
	})();		
	
	window.fbAsyncInit = function() {
		// Initialize Facebook connect.
		FB.init({
			appId: '178450008855735',		// the application id
			cookie: true,  					// enable cookies to allow the server to access the session
			xfbml: true,					// parse social plugins on this page
			version: 'v2.0',				// use version 2.1
			status: false					// get the login status.
		});
		
		/**
		// Subscribe to some events.
		FB.Event.subscribe('auth.authResponseChange', function(response) {
			console.info('*** Facebook status event: ' + response.status);
			if ( typeof(facebookStatusChangeCallback) != 'undefined' ) {
				facebookStatusChangeCallback(response);
			};
		});	
		
		FB.Event.subscribe('auth.statusChange', function(response) {
			console.info('*** Facebook status change event: ' + response.status);
			if ( typeof(facebookStatusChangeCallback) != 'undefined' ) {
				facebookStatusChangeCallback(response);
			};
		});	
		
		FB.Event.subscribe('auth.login', function(response) {
			console.info('*** Facebook login status event: ' + response.status);
			if ( typeof(facebookStatusChangeCallback) != 'undefined' ) {
				facebookStatusChangeCallback(response);
			};
		});		
		**/
		
		FB.getLoginStatus(function(response) {
			console.info('*** Facebook getLoginStatus: ' + response.status);
			if ( typeof(facebookStatusChangeCallback) != 'undefined' ) {
				facebookStatusChangeCallback(response);
			};
		}, true);
	};
	
	// Load the SDK.
	(function(d, s, id){
		var js, fjs = d.getElementsByTagName(s)[0];
		if (d.getElementById(id)) {return;}
			js = d.createElement(s); js.id = id;
			js.src = "//connect.facebook.com/en_US/sdk.js#version=v2.0&xfbml=1&appId=178450008855735";
			fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));		
