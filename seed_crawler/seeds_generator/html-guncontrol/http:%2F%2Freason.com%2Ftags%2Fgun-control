var pubservice = googletag.pubads();
function addslot(slotname, slotdim) {	
		// Desktop ads
		if ((width >= 768) && slotname.indexOf("mobile") !== 0) {
			googletag.defineSlot('/'+pubid+'/'+slotname, slotdim, 'google_ads_div_'+slotname).addService(pubservice);
		}// Mobile ads
		else if((width < 768) && slotname.indexOf("mobile") === 0){
			googletag.defineSlot('/'+pubid+'/'+slotname, slotdim, 'google_ads_div_'+slotname).addService(pubservice);
		}
}
		
var width = window.innerWidth || document.documentElement.clientWidth;
		
for (var i=0;i<slots.length;i++) {
	addslot.apply(this, slots[i]);
}
// pubservice.enableSyncRendering();
pubservice.collapseEmptyDivs();
pubservice.enableSingleRequest();
googletag.enableServices();
});
