function google_radlink_request_done(radlinks) {
    var s = "";
    if (radlinks.length == 0) {
        return;
    }
    s += '\n<div id="sponsoredLinks">\n';
    s += '\n<div id="header"><h4>Related Ads</h4><span class="whatIs"><a href="javascript:window.open(\'http://www.nytimes.com/ref/membercenter/faq/linkingqa17.html\',\'_blank\',\'width=420,height=400,scrollbars=yes,toolbars=no,resizable=no\');">What are Related Ads?</a></span>\n';
    s += '<br clear="all" /></div>';
    s += '<div id="adContent">';
    for (i = 0; i < radlinks.length; ++i) {
        s += '<div class="ad">&raquo; <a href="' + 
          
            'http://query.nytimes.com' +
          
        '/search/query?srchst=r&term=' +

            radlinks[i].term + '&google_ad_channel=topics&google_ad_client=ca-nytimes_radlinks_topic_js&google_page_url=' + escape(window.location) + '">  ' + radlinks[i].term +
            '</a></div>\n';
    }
    s += '<br clear="all"/>\n';
    s += '</div>\n';
    s += '</div>\n';
    document.write(s);
}
google_ad_channel= 'topics';
google_ad_output = 'js';
google_ad_client = 'ca-nytimes_radlinks_topic_js';
google_safe = 'high';
google_num_radlinks = '5';
google_max_num_ads = '0';
google_max_radlink_len = '23';
google_kw_type = 'broad';
</script>
