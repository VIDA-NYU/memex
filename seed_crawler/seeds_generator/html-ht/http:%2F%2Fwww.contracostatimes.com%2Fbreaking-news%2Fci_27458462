<script type="text/javascript">
// The Most-Popular addon puts a little blurb of text 
// at the bottom of an article that lets readers know
// where this article is in the most-pop rankings,
// and directs them to other most-pop content.
//
// To do this, we look up the article id against an array defined in http://extras.denverpost.com/cache/popular/contracostatimes_pop_lookup.js
// ex: http://extras.denverpost.com/cache/popular/twincities_pop_lookup.js

// Parse the article_id out from the URL
var regex = /ci_([0-9]+)/i;
var result = regex.exec(document.location);
article_id = 0;
if ( result.length > 0 ) article_id = result[1];

function to_ordinal(num)
{
    var n = num % 100;
    var suff = ["th", "st", "nd", "rd", "th"]; // suff for suffix
    var ord= n<21?(n<4 ? suff[n]:suff[0]): (n%10>4 ? suff[0] : suff[n%10]);
    if ( num == 1 ) return '<strong>#1</strong>';
    return num + ord;
}


var pop_lookups = {
    section: typeof(pop_section), section_article: 'undefined',
    full: typeof(pop_full), full_article: 'undefined',
    hour: typeof(pop_hour), hour_article: 'undefined',
    sansstate: typeof(pop_sansstate), sansstate_article: 'undefined',
    sansstate_hour: typeof(pop_sansstate_hour), sansstate_hour_article: 'undefined',
    state: typeof(pop_state), state_article: 'undefined',
    state_hour: typeof(pop_state_hour), state_hour_article: 'undefined',
    city: typeof(pop_city), city_article: 'undefined',
    sanscity: typeof(pop_sanscity), sanscity_article: 'undefined'
}

if ( pop_lookups.section != 'undefined' ) pop_lookups.section_article = typeof(pop_section[article_id]);
if ( pop_lookups.full != 'undefined' ) pop_lookups.full_article = typeof(pop_full[article_id]);
if ( pop_lookups.hour != 'undefined' ) pop_lookups.hour_article = typeof(pop_hour[article_id]);
if ( pop_lookups.sansstate != 'undefined' ) pop_lookups.sansstate_article = typeof(pop_sansstate[article_id]);
if ( pop_lookups.sansstate_hour != 'undefined' ) pop_lookups.sansstate_hour_article = typeof(pop_sansstate_hour[article_id]);
if ( pop_lookups.state != 'undefined' ) pop_lookups.state_article = typeof(pop_state[article_id]);
if ( pop_lookups.state_hour != 'undefined' ) pop_lookups.state_hour_article = typeof(pop_state_hour[article_id]);
if ( pop_lookups.city != 'undefined' ) pop_lookups.city_article = typeof(pop_city[article_id]);
if ( pop_lookups.sanscity != 'undefined' ) pop_lookups.sanscity_article = typeof(pop_sanscity[article_id]);

var pop_flag = false;
// If we have a number on the section-based lookup or the full lookup or the hour lookup,
// we know we've got something to publish.
//
// The challenge here is creating a legible sentence with the six possible combinations.
if ( pop_lookups.section_article != 'undefined' || pop_lookups.full_article != 'undefined' || pop_lookups.hour_article != 'undefined' ) 
{
    dfm.$("#the-most-popular").prepend('<h3 class="articleSubHead">This article is the:</h3>');
    if ( pop_lookups.hour_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular#popularhour">' + to_ordinal(pop_hour[article_id]) + ' most-clicked of the hour</a></li>');
    if ( pop_lookups.full_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular">' + to_ordinal(pop_full[article_id]) + ' most-clicked of the day</a></li>');
    if ( pop_lookups.state_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular#popularstate">' + to_ordinal(pop_state[article_id]) + ' most-clicked among California readers of the day</a></li>');
    if ( pop_lookups.state_hour_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular#popularstate">' + to_ordinal(pop_state_hour[article_id]) + ' most-clicked among California readers of the hour</a></li>');
    if ( pop_lookups.sansstate_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular">' + to_ordinal(pop_sansstate[article_id]) + ' most-clicked among readers outside California of the day</a></li>');
    if ( pop_lookups.sansstate_hour_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular">' + to_ordinal(pop_sansstate_hour[article_id]) + ' most-clicked among readers outside California of the hour</a></li>');
    if ( pop_lookups.city_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular#popularstate">' + to_ordinal(pop_city[article_id]) + ' most-clicked among Contra Costa readers</a></li>');
    if ( pop_lookups.section_article != 'undefined' ) dfm.$("#the-most-popular ul").append('<li><a href="/popular#pop-' + pop_section_name[article_id] + '">' + to_ordinal(pop_section[article_id]) + ' most-clicked among ' + pop_section_name[article_id] + ' articles today</a></li>');
   
    dfm.$("#the-most-popular ul").append('<li><em><a href="#top">Back to the top</a></em></li>');
    pop_flag = true;
}

</script>
 <!-- 
#############
End Most-Popular addons
#############
 -->





