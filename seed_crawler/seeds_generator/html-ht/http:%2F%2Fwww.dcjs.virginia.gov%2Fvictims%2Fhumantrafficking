<script type="text/javascript">

        $(function() {
          $('a').on('click', function (event) {
				    if ($(this).attr('href')==undefined) {
					  } else {
              if ($(this).attr('href').indexOf('http') != -1 && $(this).attr('href').indexOf(document.domain) == -1) {
                var cancel = !confirm('You are about to access a page external to dcjs.virginia.gov, do you wish to continue?');
                if (cancel) {
                  event.stopImmediatePropagation();
                  return false;
                }
              }
            }
          });
		  
          $.pnotify.defaults.styling = "jqueryui"; 
                     });
    </script>
    
